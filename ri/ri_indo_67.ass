Dialogue: 0,0:00:00.00,0:00:10.00,Default,,0,0,0,,Join channel Tele DongWorld agar tidak ketinggalan update.
Dialogue: 0,0:00:19.40,0:00:21.19,Default,,600,600,0,,Tahap awal Soul Formation
Dialogue: 0,0:00:21.20,0:00:22.63,Default,,600,600,0,,Jika saat ini maju ke depan
Dialogue: 0,0:00:22.72,0:00:23.71,Default,,600,600,0,,Pasti ada peluang besar
Dialogue: 0,0:00:45.32,0:00:45.71,Default,,600,600,0,,Tuan
Dialogue: 0,0:00:45.92,0:00:46.99,Default,,600,600,0,,Jangan buat aku terkejut!
Dialogue: 0,0:00:47.32,0:00:48.51,Default,,600,600,0,,Kekuatan ilahiku melemah
Dialogue: 0,0:00:49.16,0:00:50.71,Default,,600,600,0,,Aku harus segera pulih
Dialogue: 0,0:00:57.92,0:00:58.63,Default,,600,600,0,,Rekan Dao
Dialogue: 0,0:00:58.92,0:01:00.03,Default,,600,600,0,,Apa yang terjadi?
Dialogue: 0,0:01:00.36,0:01:01.75,Default,,600,600,0,,Sehingga kau jadi seperti ini?
Dialogue: 0,0:01:03.60,0:01:04.51,Default,,600,600,0,,Ras raksasa
Dialogue: 0,0:01:09.84,0:01:11.39,Default,,600,600,0,,Pecahan Dunia Abadi runtuh
Dialogue: 0,0:01:13.72,0:01:14.31,Default,,600,600,0,,Rekan Dao
Dialogue: 0,0:01:14.52,0:01:15.95,Default,,600,600,0,,Pecahan Dunia Abadi ini runtuh
Dialogue: 0,0:01:16.16,0:01:18.31,Default,,600,600,0,,Yang bisa lolos pasti bukan orang sembarangan
Dialogue: 0,0:01:19.20,0:01:20.75,Default,,600,600,0,,Namaku Chi Hu dari ras raksasa
Dialogue: 0,0:01:21.52,0:01:22.11,Default,,600,600,0,,Tidak tahu,
Dialogue: 0,0:01:22.24,0:01:23.39,Default,,600,600,0,,Rekan Dao dipanggil apa?
Dialogue: 0,0:01:23.76,0:01:25.17,Default,,600,600,0,,Saya, Zeng Niu
Dialogue: 0,0:01:26.72,0:01:27.55,Default,,600,600,0,,Zeng Niu
Dialogue: 0,0:01:29.04,0:01:30.31,Default,,600,600,0,,Di atas Bintang Vermillion
Dialogue: 0,0:01:30.72,0:01:32.31,Default,,600,600,0,,Hanya ada sekte tinggi bernama Zeng
Dialogue: 0,0:01:32.52,0:01:34.23,Default,,600,600,0,,Sekte Tianhun
Dialogue: 0,0:01:35.32,0:01:37.79,Default,,600,600,0,,Sejak muda sudah tahap awal Soul Formation
Dialogue: 0,0:01:38.12,0:01:40.19,Default,,600,600,0,,Dan bisa mengendalikan jiwa di dunia kekosongan
Dialogue: 0,0:01:40.92,0:01:43.91,Default,,600,600,0,,Pasti adalah murid inti dari negara kultivasi tingkat 5
Dialogue: 0,0:01:44.92,0:01:45.71,Default,,600,600,0,,Rekan Dao,
Dialogue: 0,0:01:46.16,0:01:47.59,Default,,600,600,0,,Bolehkah kita bicara sebentar?
Dialogue: 0,0:01:59.40,0:02:00.15,Default,,600,600,0,,Saudara Zeng,
Dialogue: 0,0:02:00.76,0:02:02.15,Default,,600,600,0,,langsung ke intinya,
Dialogue: 0,0:02:02.56,0:02:03.83,Default,,600,600,0,,kita sama-sama dari Bintang Vermillion,
Dialogue: 0,0:02:04.52,0:02:05.75,Default,,600,600,0,,tolong bantu aku,
Dialogue: 0,0:02:07.24,0:02:08.51,Default,,600,600,0,,Rekan Dao lihatlah sendiri
Dialogue: 0,0:02:18.04,0:02:19.91,Default,,600,600,0,,Batu Giok Abadi...
Dialogue: 0,0:02:21.00,0:02:23.43,Default,,600,600,0,,Dunia Abadi masih ada tempat seperti ini
Dialogue: 0,0:02:24.00,0:02:24.79,Default,,600,600,0,,Saudara Zeng,
Dialogue: 0,0:02:25.20,0:02:26.91,Default,,600,600,0,,Tempat ini adalah milik para senior ras raksasa kami
Dialogue: 0,0:02:27.36,0:02:29.11,Default,,600,600,0,,Yang masuk ke Dunia Abadi bertahun-tahun lalu
Dialogue: 0,0:02:29.12,0:02:30.43,Default,,600,600,0,,Tanpa sengaja terjebak
Dialogue: 0,0:02:31.36,0:02:31.91,Default,,600,600,0,,Tempat ini,
Dialogue: 0,0:02:32.20,0:02:33.51,Default,,600,600,0,,Dijaga oleh Binatang Abadi
Dialogue: 0,0:02:33.72,0:02:34.15,Default,,600,600,0,,Sementara itu,
Dialogue: 0,0:02:34.60,0:02:35.91,Default,,600,600,0,,Ada banyak larangan
Dialogue: 0,0:02:36.24,0:02:37.51,Default,,600,600,0,,Dengan kekuatan dia sendiri,
Dialogue: 0,0:02:37.52,0:02:38.75,Default,,600,600,0,,tidak mungkin bertindak sembarangan
Dialogue: 0,0:02:39.16,0:02:41.31,Default,,600,600,0,,Hanya merekam semuanya dengan kesadarannya,
Dialogue: 0,0:02:41.32,0:02:42.51,Default,,600,600,0,,lalu diam-diam kembali
Dialogue: 0,0:02:44.04,0:02:45.03,Default,,600,600,0,,Setelah bertahun-tahun
Dialogue: 0,0:02:45.40,0:02:47.43,Default,,600,600,0,,Tempat ini pasti sudah ditemukan
Dialogue: 0,0:02:48.64,0:02:49.31,Default,,600,600,0,,Tidak mungkin
Dialogue: 0,0:02:49.80,0:02:51.75,Default,,600,600,0,,Tempat ini pasti tidak ditemukan
Dialogue: 0,0:02:52.72,0:02:54.23,Default,,600,600,0,,Namun, alasannya...
Dialogue: 0,0:02:54.92,0:02:56.79,Default,,600,600,0,,Aku tidak dapat memberitahu sekarang
Dialogue: 0,0:02:57.84,0:02:58.83,Default,,600,600,0,,Saudara Zeng, harap dimaklumi
Dialogue: 0,0:03:00.00,0:03:01.83,Default,,600,600,0,,Chi Hu tidak mengenaliku,
Dialogue: 0,0:03:02.32,0:03:03.71,Default,,600,600,0,,Tapi kini mengajakku berkelompok
Dialogue: 0,0:03:04.44,0:03:05.55,Default,,600,600,0,,Hanya karena,
Dialogue: 0,0:03:05.80,0:03:07.31,Default,,600,600,0,,Terpikat pada jiwa-jiwaku
Dialogue: 0,0:03:07.72,0:03:09.47,Default,,600,600,0,,Tempat seperti ini, hanya mengandalkan kita berdua,
Dialogue: 0,0:03:10.04,0:03:11.71,Default,,600,600,0,,sulit untuk berhasil masuk
Dialogue: 0,0:03:12.04,0:03:12.99,Default,,600,600,0,,Tenang saja, Rekan Dao.
Dialogue: 0,0:03:13.52,0:03:14.51,Default,,600,600,0,,Selain kamu,
Dialogue: 0,0:03:15.12,0:03:16.43,Default,,600,600,0,,Aku juga mengajak seseorang
Dialogue: 0,0:03:16.92,0:03:18.15,Default,,600,600,0,,Kami bertiga berangkat bersama
Dialogue: 0,0:03:18.44,0:03:19.99,Default,,600,600,0,,Pasti lebih berpeluang
Dialogue: 0,0:03:20.96,0:03:22.31,Default,,600,600,0,,Tetapi Saudara Zeng jangan khawatir
Dialogue: 0,0:03:22.64,0:03:23.95,Default,,600,600,0,,Nanti semua harta
Dialogue: 0,0:03:24.12,0:03:25.51,Default,,600,600,0,,Aku akan membagi dengan adil
Dialogue: 0,0:03:26.60,0:03:27.75,Default,,600,600,0,,Saudara Zeng pikirkan saja dulu
Dialogue: 0,0:03:29.16,0:03:30.19,Default,,600,600,0,,Jika ingin datang,
Dialogue: 0,0:03:30.36,0:03:31.35,Default,,600,600,0,,sebulan kemudian,
Dialogue: 0,0:03:31.44,0:03:33.91,Default,,600,600,0,,Ke tempat berkumpul peta giok ini
Dialogue: 0,0:03:38.72,0:03:39.51,Default,,600,600,0,,Saudara Zeng
Dialogue: 0,0:03:39.88,0:03:41.91,Default,,600,600,0,,Aku menanti kehadiranmu
Dialogue: 0,0:03:45.24,0:03:46.63,Default,,600,600,0,,Sebelum luka pulih,
Dialogue: 0,0:03:47.12,0:03:48.91,Default,,600,600,0,,tidak boleh mengambil risiko
Dialogue: 0,0:03:52.91,0:03:54.60,Default,,600,600,350,,{\i1}Beberapa hari kemudian...{\i0}
Dialogue: 0,0:04:00.52,0:04:01.67,Default,,600,600,0,,Energi Abadi
Dialogue: 0,0:04:02.72,0:04:05.55,Default,,500,500,0,,Benar-benar harus menunggu hingga tahap akhir Soul Transformation,
Dialogue: 0,0:04:05.84,0:04:07.15,Default,,600,600,0,,Baru bisa digunakan
Dialogue: 0,0:04:08.40,0:04:11.31,Default,,600,600,0,,Jika dimanfaatkan sebelum waktunya justru akan merusak jiwa
Dialogue: 0,0:04:12.24,0:04:14.51,Default,,600,600,0,,Untungnya hasil kali ini cukup besar
Dialogue: 0,0:04:15.64,0:04:17.31,Default,,600,600,0,,Giok Abadi ini sempurna
Dialogue: 0,0:04:18.12,0:04:19.59,Default,,600,600,0,,Energi Abadi yang ada
Dialogue: 0,0:04:19.95,0:04:21.86,Default,,600,600,0,,Lebih kental dibanding kristal yang diproses manusia
Dialogue: 0,0:04:22.91,0:04:24.58,Default,,0,0,0,,Latihan setelah tahap Transformasi Awal
Dialogue: 0,0:04:25.12,0:04:27.03,Default,,600,600,0,,Benda ini tentu lebih baik lebih banyak
Dialogue: 0,0:04:29.12,0:04:30.59,Default,,600,600,0,,Namun benda ini terlihat
Dialogue: 0,0:04:31.12,0:04:33.11,Default,,600,600,0,,Sepertinya bukan artefak pertahanan
Dialogue: 0,0:04:42.92,0:04:44.23,Default,,600,600,0,,Metode yang sangat halus
Dialogue: 0,0:04:45.12,0:04:47.71,Default,,600,600,0,,Ternyata benda ini adalah giok kuno yang diukir oleh Immortal
Dialogue: 0,0:05:00.92,0:05:03.35,Default,,600,600,0,,Namaku Tianbao Shangren
Dialogue: 0,0:05:04.16,0:05:07.11,Default,,600,600,0,,Membuat artefak terkuat di antara langit dan bumi...
Dialogue: 0,0:05:07.92,0:05:10.71,Default,,600,600,0,,adalah keinginanku seumur hidup
Dialogue: 0,0:05:11.92,0:05:15.33,Default,,600,600,0,,Sayangnya, bencana datang tiba-tiba
Dialogue: 0,0:05:15.80,0:05:19.03,Default,,600,600,0,,Seluruh usaha dan pengorbananku tidak dapat diteruskan
Dialogue: 0,0:05:19.44,0:05:22.99,Default,,600,600,0,,Hanya bisa menyimpan semua harta,
Dialogue: 0,0:05:23.72,0:05:28.23,Default,,600,600,0,,untuk orang-orang yang memiliki nasib baik bisa menemui giok ini
Dialogue: 0,0:05:29.32,0:05:31.55,Default,,600,600,0,,Silakan datang untuk mengambilnya
Dialogue: 0,0:05:32.72,0:05:33.99,Default,,600,600,0,,Harta letusan
Dialogue: 0,0:05:34.36,0:05:35.39,Default,,600,600,0,,Artefak semacam ini,
Dialogue: 0,0:05:35.80,0:05:37.91,Default,,600,600,0,,mungkin masih ada di gua Tianbao Shangren
Dialogue: 0,0:05:38.32,0:05:39.91,Default,,600,600,0,,Harta yang terbuat dari material Abadi
Dialogue: 0,0:05:40.04,0:05:41.71,Default,,600,600,0,,Tidak akan bisa dibandingkan dengan artefak dunia bawah
Dialogue: 0,0:05:42.52,0:05:43.39,Default,,600,600,0,,Kesempatan ini,
Dialogue: 0,0:05:44.08,0:05:45.19,Default,,600,600,0,,tidak boleh dilewatkan
Dialogue: 0,0:05:48.19,0:05:51.00,Default,,0,0,350,,{\i1}Sepuluh hari kemudian...{\i0}
Dialogue: 0,0:05:53.52,0:05:55.91,Default,,600,600,0,,Pecahan Dunia Abadi yang tercatat di Sekte Daluo Sword,
Dialogue: 0,0:05:56.12,0:05:57.51,Default,,600,600,0,,semua sudah dicari
Dialogue: 0,0:05:58.40,0:05:59.39,Default,,600,600,0,,Dunia Abadi sangat luas
Dialogue: 0,0:06:00.12,0:06:02.31,Default,,600,600,0,,Jika gua tidak dalam jangkauan peta giok
Dialogue: 0,0:06:02.72,0:06:04.51,Default,,600,600,0,,Berapa lama lagi harus mencarinya
Dialogue: 0,0:06:09.32,0:06:09.91,Default,,600,600,0,,Rekan Dao,
Dialogue: 0,0:06:10.16,0:06:10.79,Default,,600,600,0,,selamatkan aku!
Dialogue: 0,0:06:21.32,0:06:22.11,Default,,600,600,0,,Rekan Dao,
Dialogue: 0,0:06:22.36,0:06:23.55,Default,,600,600,0,,masalah ini tidak ada hubungannya denganmu
Dialogue: 0,0:06:23.76,0:06:24.63,Default,,600,600,0,,Silakan pergi!
Dialogue: 0,0:06:26.44,0:06:28.43,Default,,600,600,0,,Orang ini berusaha merebut kristal energiku
Dialogue: 0,0:06:28.72,0:06:29.91,Default,,600,600,0,,Tolong bantu saya bunuh bajingan ini
Dialogue: 0,0:06:30.16,0:06:31.31,Default,,600,600,0,,Kristal ini bagi kita berdua
Dialogue: 0,0:06:31.34,0:06:32.24,Default,,0,0,0,,Bacot!
Dialogue: 0,0:06:32.92,0:06:34.71,Default,,600,600,0,,Kristal ini adalah hasil usahaku yang keras
Dialogue: 0,0:06:40.08,0:06:41.67,Default,,600,600,0,,Kita semua dari Bintang Vermillion,
Dialogue: 0,0:06:41.80,0:06:42.71,Default,,600,600,0,,bantulah aku sekali!
Dialogue: 0,0:06:43.00,0:06:43.59,Default,,600,600,0,,Pergi!
Dialogue: 0,0:06:43.96,0:06:45.03,Default,,600,600,0,,Masalah kalian berdua,
Dialogue: 0,0:06:45.24,0:06:46.91,Default,,600,600,0,,jangan seret aku ke dalamnya
Dialogue: 0,0:06:49.32,0:06:50.71,Default,,600,600,0,,Kalau begitu aku akan,
Dialogue: 0,0:06:51.20,0:06:53.35,Default,,600,600,0,,mengalihkan masalah ini kepada kalian
Dialogue: 0,0:06:53.72,0:06:54.47,Default,,600,600,0,,Kristal ini,
Dialogue: 0,0:06:54.60,0:06:55.87,Default,,600,600,0,,aku tidak mau!
Dialogue: 0,0:07:11.72,0:07:12.35,Default,,600,600,0,,Terima kasih
Dialogue: 0,0:07:13.04,0:07:14.39,Default,,600,600,0,,Namaku Sima Yun dari Sekte Kui Shui
Dialogue: 0,0:07:14.92,0:07:15.91,Default,,600,600,0,,Tidak tahu siapa Rekan Dao...
Dialogue: 0,0:07:16.80,0:07:17.63,Default,,600,600,0,,Zeng Niu
Dialogue: 0,0:07:18.72,0:07:19.31,Default,,600,600,0,,Saudara Zeng,
Dialogue: 0,0:07:19.84,0:07:21.51,Default,,600,600,0,,aku sudah mencari tempat ini selama beberapa hari
Dialogue: 0,0:07:21.92,0:07:23.35,Default,,600,600,0,,Tidak ada lagi energi Abadi yang tersisa
Dialogue: 0,0:07:24.20,0:07:25.31,Default,,600,600,0,,Tak perlu membuang waktu lagi
Dialogue: 0,0:07:25.72,0:07:27.31,Default,,600,600,0,,Tidak tahu apakah ada titik teleportasi
Dialogue: 0,0:07:27.72,0:07:28.35,Default,,600,600,0,,Jika ada,
Dialogue: 0,0:07:28.76,0:07:30.15,Default,,600,600,0,,mungkin kita bisa bertukar informasi
Dialogue: 0,0:07:31.20,0:07:34.31,Default,,600,600,0,,Orang ini juga mungkin membawa peta teleportasi yang diberikan sekte
Dialogue: 0,0:07:36.92,0:07:38.51,Default,,600,600,0,,Kau ingin menukarnya dengan apa?
Dialogue: 0,0:07:47.16,0:07:48.59,Default,,600,600,0,,Pecahan yang dimiliki orang ini,
Dialogue: 0,0:07:49.20,0:07:50.95,Default,,600,600,0,,ada lima di bagian barat
Dialogue: 0,0:07:51.12,0:07:51.75,Default,,600,600,0,,Saudara Zeng
Dialogue: 0,0:07:52.20,0:07:53.91,Default,,600,600,0,,Peta ini memiliki dua puluh satu titik
Dialogue: 0,0:07:54.52,0:07:55.99,Default,,600,600,0,,Tak termasuk yang kau punya
Dialogue: 0,0:07:56.20,0:07:57.19,Default,,600,600,0,,Setelah kau pilih beberapa
Dialogue: 0,0:07:57.40,0:07:58.35,Default,,600,600,0,,Bagaimana kalau kita tukar?
Dialogue: 0,0:08:08.12,0:08:08.59,Default,,600,600,0,,Saudara Zeng
Dialogue: 0,0:08:09.20,0:08:11.03,Default,,600,600,0,,Jika ada kesempatan ke Bintang Shiyi di masa depan
Dialogue: 0,0:08:11.20,0:08:12.23,Default,,600,600,0,,Aku pasti akan menjamu
Dialogue: 0,0:08:12.96,0:08:13.75,Default,,600,600,0,,Baiklah
Dialogue: 0,0:08:13.96,0:08:14.79,Default,,600,600,0,,Aku pamit
Dialogue: 0,0:08:15.24,0:08:17.03,Default,,600,600,0,,Bajingan kecil itu berani merebut milikku
Dialogue: 0,0:08:17.32,0:08:18.91,Default,,600,600,0,,Aku tidak akan membiarkannya lolos
Dialogue: 0,0:08:21.08,0:08:22.03,Default,,600,600,0,,Begitu melihat ini,
Dialogue: 0,0:08:22.60,0:08:24.19,Default,,600,600,0,,dicatat oleh Sekte Daluo Sword,
Dialogue: 0,0:08:24.96,0:08:27.23,Default,,600,600,0,,sepertinya hanya puncak gunung es di Dunia Abadi!
Dialogue: 0,0:08:41.56,0:08:42.23,Default,,600,600,0,,Akhirnya ditemukan
Dialogue: 0,0:08:45.00,0:08:46.11,Default,,600,600,0,,Puncak Gunung Harimau
Dialogue: 0,0:08:46.52,0:08:47.55,Default,,600,600,0,,Ada tempat seperti itu
Dialogue: 0,0:09:12.72,0:09:14.23,Default,,600,600,0,,Ternyata sudah pernah dicari orang sebelumnya
Dialogue: 0,0:09:15.40,0:09:17.51,Default,,600,600,0,,Sia-sia jauh datang kesini.
Dialogue: 0,0:09:34.40,0:09:35.55,Default,,600,600,0,,Harta letusan
Dialogue: 0,0:09:36.12,0:09:39.07,Default,,600,600,0,,Tersembunyi di ruangan kesepuluh gua
Dialogue: 0,0:09:39.64,0:09:41.39,Default,,600,600,0,,Diberikan kepada orang yang bernasib baik
Dialogue: 0,0:09:41.92,0:09:44.03,Default,,600,600,0,,Tempat ini jelas hanya memiliki sembilan ruangan rahasia
Dialogue: 0,0:09:44.84,0:09:46.43,Default,,600,600,0,,Dari mana ruangan kesepuluh ini?
Dialogue: 0,0:09:46.76,0:09:50.35,Default,,600,600,0,,Harta letusan adalah hasil usahaku seumur hidup
Dialogue: 0,0:09:50.84,0:09:52.59,Default,,600,600,0,,Jika bisa terlihat kembali ke dunia
Dialogue: 0,0:09:55.00,0:09:56.91,Default,,600,600,0,,Kehidupan ini tidak sia-sia
Dialogue: 0,0:10:02.00,0:10:03.31,Default,,600,600,0,,Gua ini terus tenggelam
Dialogue: 0,0:10:04.44,0:10:05.91,Default,,600,600,0,,Meskipun telah ditemukan oleh orang lain,
Dialogue: 0,0:10:06.74,0:10:09.65,Default,,600,600,0,,pada akhirnya, sangat sulit menemukan gua harta yang sebenarnya
Dialogue: 0,0:10:30.04,0:10:31.19,Default,,600,600,0,,Tidak tahu apa yang ada di ruangan rahasia itu
Dialogue: 0,0:10:31.64,0:10:33.03,Default,,600,600,0,,Apa rahasianya ya?
Dialogue: 0,0:10:44.72,0:10:45.91,Default,,600,600,0,,Bentuknya menyerupai Binatang Ilahi
Dialogue: 0,0:10:47.24,0:10:48.43,Default,,600,600,0,,Seolah hidup
Dialogue: 0,0:10:50.20,0:10:51.19,Default,,600,600,0,,Inilah
Dialogue: 0,0:10:51.52,0:10:52.83,Default,,600,600,0,,Harta letusan
Dialogue: 0,0:11:07.84,0:11:09.03,Default,,600,600,0,,Kekuatan yang sangat mengesankan
Dialogue: 0,0:11:10.00,0:11:11.43,Default,,600,600,0,,Hanya dengan roh binatang terkecilnya
Dialogue: 0,0:11:12.00,0:11:14.83,Default,,600,600,0,,Lebih kuat daripada Naga Sembilan Kepala di Laut Ilahi Kuno
Dialogue: 0,0:11:15.40,0:11:17.95,Default,,600,600,0,,Tak heran meski potongan logam ini tak mencatat nama binatang itu
Dialogue: 0,0:11:18.76,0:11:20.03,Default,,600,600,0,,Tetapi khusus menyebutkan
Dialogue: 0,0:11:20.04,0:11:21.75,Default,,600,600,0,,Roh binatang ini terkurung oleh rantai jiwa
Dialogue: 0,0:11:22.96,0:11:24.71,Default,,600,600,0,,Tidak bisa melarikan diri lebih dari tiga kaki
Dialogue: 0,0:11:30.44,0:11:31.95,Default,,600,600,0,,Untuk menggunakan harta letusan ini,
Dialogue: 0,0:11:32.24,0:11:33.50,Default,,600,600,0,,dengan kekuatan Soul Transformation-ku
Dialogue: 0,0:11:34.00,0:11:35.19,Default,,600,600,0,,Mustahil untuk kulakukan
Dialogue: 0,0:11:36.36,0:11:38.63,Default,,600,600,0,,Hanya bisa membuat binatang ini tertidur sementara
Dialogue: 0,0:11:47.96,0:11:49.95,Default,,600,600,0,,Tentu Tianbao Shangren juga tidak akan menyangka
Dialogue: 0,0:11:50.64,0:11:52.83,Default,,600,600,0,,Harta sepenting ini bisa jatuh ke tanganku
Dialogue: 0,0:11:53.48,0:11:55.79,Default,,600,600,0,,Di tahap awal Soul Transformation seperti ini
Dialogue: 0,0:12:16.04,0:12:17.83,Default,,600,600,0,,Harta letusan ini memang sangat luar biasa
Dialogue: 0,0:12:18.80,0:12:20.59,Default,,600,600,0,,Sama sekali menggunakan roh binatang sebagai sumber tenaga
Dialogue: 0,0:12:21.52,0:12:22.43,Default,,600,600,0,,Kekuatan ini
Dialogue: 0,0:12:22.80,0:12:24.35,Default,,600,600,0,,Pasti tidak akan mengecewakanku
Dialogue: 0,0:12:25.52,0:12:26.95,Default,,600,600,0,,Binatang ini bentuknya paling kecil
Dialogue: 0,0:12:27.64,0:12:29.67,Default,,600,600,0,,Mungkin cincin penjinak binatang bisa menawannya
Dialogue: 0,0:12:30.44,0:12:32.15,Default,,600,600,0,,Tetapi jika tidak ada harta letusan sebagai perisai
Dialogue: 0,0:12:32.40,0:12:33.83,Default,,600,600,0,,Langsung menyerap kekuatanku
Dialogue: 0,0:12:34.40,0:12:36.43,Default,,600,600,0,,Apakah aku bisa bertahan?
Dialogue: 0,0:12:43.92,0:12:45.23,Default,,600,600,0,,Perjalanan ke Dunia Abadi ini
Dialogue: 0,0:12:45.64,0:12:47.19,Default,,600,600,0,,Hasilnya cukup berharga
Dialogue: 0,0:12:48.16,0:12:49.75,Default,,600,600,0,,Namun risikonya juga sangat besar
Dialogue: 0,0:12:50.24,0:12:52.83,Default,,600,600,0,,Jika sedikit ceroboh saat  pecahan Dunia Abadi itu runtuh,
Dialogue: 0,0:12:53.04,0:12:54.63,Default,,600,600,0,,mungkin sekarang sudah tiada
Dialogue: 0,0:12:56.04,0:12:56.95,Default,,600,600,0,,Dengan kata lain
Dialogue: 0,0:12:57.36,0:12:58.55,Default,,600,600,0,,Undangan Chi Hu
Dialogue: 0,0:12:59.16,0:13:01.19,Default,,600,600,0,,Harus dipertimbangkan baik-baik
Dialogue: 0,0:13:01.92,0:13:03.63,Default,,600,600,0,,Belum lagi tiga pedang abadi
Dialogue: 0,0:13:04.36,0:13:05.83,Default,,600,600,0,,Hanya dari banyak giok abadi
Dialogue: 0,0:13:06.20,0:13:08.43,Default,,600,600,0,,Sangat penting untukku mencapai awal Soul Transformation nanti
Dialogue: 0,0:13:09.32,0:13:10.71,Default,,600,600,0,,Begitu kekuatanku mencapai Soul Transformation
Dialogue: 0,0:13:11.24,0:13:12.83,Default,,600,600,0,,Tu Si bisa bangkit
Dialogue: 0,0:13:13.84,0:13:15.43,Default,,600,600,0,,Dan aku bisa membawa sedikit ramuan ilahi
Dialogue: 0,0:13:16.16,0:13:18.15,Default,,600,600,0,,Untuk dibawa pulang kepada Wan'er
Dialogue: 0,0:13:30.60,0:13:31.23,Default,,600,600,0,,Dia datang
Dialogue: 0,0:13:31.68,0:13:32.59,Default,,600,600,0,,Tapi tidak tahu
Dialogue: 0,0:13:32.84,0:13:33.91,Default,,600,600,0,,Siapa dia
Dialogue: 0,0:13:51.44,0:13:52.31,Default,,600,600,0,,Rekan Hong Die
Dialogue: 0,0:13:52.48,0:13:53.87,Default,,600,600,0,,Aku menantimu sudah lama
Dialogue: 0,0:13:56.12,0:13:57.47,Default,,600,600,0,,Jika apa yang kau katakan palsu
Dialogue: 0,0:13:58.40,0:14:00.31,Default,,600,600,0,,Hal ini tidak akan dibiarkan begitu saja
Dialogue: 0,0:14:00.84,0:14:02.11,Default,,600,600,0,,Rekan Hong Die tenang saja
Dialogue: 0,0:14:02.48,0:14:03.23,Default,,600,600,0,,Giok ini
Dialogue: 0,0:14:03.36,0:14:05.99,Default,,600,600,0,,Adalah sesuai yang dicetak langsung oleh nenek moyang ras raksasa
Dialogue: 0,0:14:07.24,0:14:09.31,Default,,600,600,0,,Dia tidak mungkin Bohong
Dialogue: 0,0:14:10.40,0:14:11.23,Default,,600,600,0,,Nenek moyang
Dialogue: 0,0:14:12.56,0:14:13.43,Default,,600,600,0,,Dari Bintang Vermillion
Dialogue: 0,0:14:13.72,0:14:15.43,Default,,600,600,0,,Sekte yang ada dalam bentuk klan
Dialogue: 0,0:14:15.64,0:14:17.23,Default,,600,600,0,,Hanya ada kamu dan aku di luar sana
Dialogue: 0,0:14:18.24,0:14:20.47,Default,,600,600,0,,Nenek moyangku dan gurumu adalah teman baik
Dialogue: 0,0:14:20.56,0:14:22.63,Default,,600,600,0,,Bagaimana mungkin aku akan menipu kamu?
Dialogue: 0,0:14:24.32,0:14:26.11,Default,,600,600,0,,Karena nenek moyangmu melihat sendiri
Dialogue: 0,0:14:26.64,0:14:27.95,Default,,600,600,0,,Aku bisa ikut denganmu
Dialogue: 0,0:14:29.04,0:14:30.39,Default,,600,600,0,,Tetapi apa pun yang kita dapatkan
Dialogue: 0,0:14:30.40,0:14:31.67,Default,,600,600,0,,Giok ini
Dialogue: 0,0:14:32.08,0:14:33.15,Default,,600,600,0,,Harus menjadi milikku
Dialogue: 0,0:14:36.24,0:14:38.43,Default,,600,600,0,,Sungguh perempuan yang angkuh
Dialogue: 0,0:14:40.28,0:14:41.31,Default,,600,600,0,,Rekan Hong Die
Dialogue: 0,0:14:41.92,0:14:43.27,Default,,600,600,0,,Hal ini tidak adil
Dialogue: 0,0:14:44.00,0:14:45.35,Default,,600,600,0,,Karena selain kalian berdua
Dialogue: 0,0:14:45.44,0:14:46.35,Default,,600,600,0,,Ada satu orang lagi
Dialogue: 0,0:14:46.84,0:14:48.35,Default,,600,600,0,,Seharusnya juga akan terlibat dalam hal ini
Dialogue: 0,0:14:48.68,0:14:49.83,Default,,600,600,0,,Aku sudah menjanjikan itu
Dialogue: 0,0:14:50.32,0:14:52.11,Default,,600,600,0,,Jika kita bertiga bergabung
Dialogue: 0,0:14:52.32,0:14:53.27,Default,,600,600,0,,Masing-masing memiliki kesempatan
Dialogue: 0,0:14:53.96,0:14:54.83,Default,,600,600,0,,Dalam hal keadilan
Dialogue: 0,0:14:55.24,0:14:56.83,Default,,600,600,0,,Semua harta yang diperoleh harus dibagi rata
Dialogue: 0,0:14:58.36,0:15:00.35,Default,,600,600,0,,Sebagai ketua cabang ras raksasa
Dialogue: 0,0:15:00.68,0:15:01.95,Default,,600,600,0,,Aku pasti menepati janji
Dialogue: 0,0:15:03.60,0:15:04.31,Default,,600,600,0,,Siapa?
Dialogue: 0,0:15:05.07,0:15:05.80,Default,,0,0,0,,Zeng Niu
Dialogue: 0,0:15:07.16,0:15:08.15,Default,,600,600,0,,Seperti kita
Dialogue: 0,0:15:08.64,0:15:09.83,Default,,600,600,0,,Semua berasal dari Bintang Vermillion
Dialogue: 0,0:15:10.62,0:15:11.68,Default,,0,0,0,,dari Bintang Vermillion...
Dialogue: 0,0:15:14.72,0:15:15.27,Default,,600,600,0,,Saudara Zeng
Dialogue: 0,0:15:15.48,0:15:16.67,Default,,600,600,0,,Kau datang tepat pada waktunya
Dialogue: 0,0:15:16.96,0:15:17.79,Default,,600,600,0,,Itu kau! 
