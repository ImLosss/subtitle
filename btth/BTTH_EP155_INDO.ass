Dialogue: 0,0:13:28.56,0:13:30.94,komen,,530,0,420,,{\fad(500,250)}[Xiao Xuan]
Dialogue: 0,0:08:25.18,0:08:28.02,komen,,0,1250,470,,{\fad(500,250)}[Hun Diao]
Dialogue: 0,0:03:16.35,0:03:17.51,btth,,0,0,0,,Seribu tahun yang lalu,
Dialogue: 0,0:03:17.63,0:03:19.55,btth,,0,0,0,,Penyebab darah Dou Di di Klan Xiao habis.
Dialogue: 0,0:03:19.63,0:03:22.55,btth,,0,0,0,,Ketua Klan Xiao, Xiao Xuan, mengambil langkah terakhir.
Dialogue: 0,0:03:22.55,0:03:24.47,btth,,0,0,0,,Mengumpulkan seluruh darah klan pada dirinya,
Dialogue: 0,0:03:24.47,0:03:26.55,btth,,0,0,0,,bertekad untuk menerobos Dou Di.
Dialogue: 0,0:03:26.91,0:03:27.63,btth,,0,0,0,,Sayangnya,
Dialogue: 0,0:03:27.91,0:03:30.43,btth,,0,0,0,,Xiao Xuan akhirnya gagal menerobos.
Dialogue: 0,0:03:30.43,0:03:33.11,btth,,0,0,0,,Kemudian diserang secara tiba-tiba oleh Klan Hun.
Dialogue: 0,0:03:33.15,0:03:34.07,btth,,0,0,0,,Sejak saat itu,
Dialogue: 0,0:03:34.15,0:03:36.47,btth,,0,0,0,,Klan Xiao benar-benar merosot.
Dialogue: 0,0:03:36.55,0:03:38.35,btth,,0,0,0,,Untuk menemukan leluhur Xiao Xuan,
Dialogue: 0,0:03:38.47,0:03:40.63,btth,,0,0,0,,Xiao Yan melewati berbagai rintangan.
Dialogue: 0,0:03:40.63,0:03:43.15,btth,,0,0,0,,Akhirnya tiba di lapisan ketiga Makam Langit.
Dialogue: 0,0:03:43.67,0:03:44.47,btth,,0,0,0,,Tapi tak disangka,
Dialogue: 0,0:03:44.47,0:03:47.63,btth,,0,0,0,,diserang oleh tubuh energi setingkat Ban Sheng.
Dialogue: 0,0:03:47.75,0:03:49.43,btth,,0,0,0,,Nyawanya terancam.
Dialogue: 0,0:04:05.35,0:04:06.15,btth,,0,0,0,,Kau...
Dialogue: 0,0:04:06.63,0:04:08.11,btth,,0,0,0,,kau orang Klan Xiao?
Dialogue: 0,0:04:08.75,0:04:09.91,btth,,0,0,0,,Bagaimana kau tahu?
Dialogue: 0,0:04:12.71,0:04:13.51,btth,,0,0,0,,Apa yang terjadi?
Dialogue: 0,0:04:14.27,0:04:15.03,btth,,0,0,0,,Tidak tahu.
Dialogue: 0,0:04:15.11,0:04:15.91,btth,,0,0,0,,Hati-hati.
Dialogue: 0,0:04:17.51,0:04:18.51,btth,,0,0,0,,Tanda klan itu.
Dialogue: 0,0:04:18.71,0:04:20.07,btth,,0,0,0,,Aku takkan salah mengenalinya.
Dialogue: 0,0:04:23.75,0:04:24.51,btth,,0,0,0,,Bocah ini,
Dialogue: 0,0:04:24.51,0:04:26.23,btth,,0,0,0,,ternyata orang Klan Xiao.
Dialogue: 0,0:04:26.63,0:04:28.47,btth,,0,0,0,,Dengan kemampuan luar biasa orang itu,
Dialogue: 0,0:04:28.51,0:04:31.31,btth,,0,0,0,,pasti bisa merasakannya sejak bocah ini masuk Makam Langit.
Dialogue: 0,0:04:32.03,0:04:33.43,btth,,0,0,0,,Jika aku membunuhnya.
Dialogue: 0,0:04:33.47,0:04:35.11,btth,,0,0,0,,Nyawaku bisa terancam.
Dialogue: 0,0:04:39.83,0:04:40.63,btth,,0,0,0,,Pergi!
Dialogue: 0,0:04:40.95,0:04:42.63,btth,,0,0,0,,Kali ini kalian beruntung.
Dialogue: 0,0:04:45.15,0:04:46.15,btth,,0,0,0,,Apa kalian bengong?
Dialogue: 0,0:04:46.51,0:04:47.75,btth,,0,0,0,,Dari sini ke barat,
Dialogue: 0,0:04:47.75,0:04:49.11,btth,,0,0,0,,bisa menghindari orang lain,
Dialogue: 0,0:04:49.11,0:04:50.63,btth,,0,0,0,,dan mencapai tempat tujuan kalian.
Dialogue: 0,0:04:56.63,0:04:57.51,btth,,0,0,0,,Terima kasih, Senior.
Dialogue: 0,0:05:06.71,0:05:08.95,btth,,0,0,0,,Kali ini kau berutang padaku.
Dialogue: 0,0:05:09.55,0:05:11.31,btth,,0,0,0,,Lain kali jika aku membersihkan area.
Dialogue: 0,0:05:11.47,0:05:13.11,btth,,0,0,0,,Harusnya kau lepaskan aku.
Dialogue: 0,0:05:21.87,0:05:23.15,btth,,0,0,0,,Tubuh energi Ban Sheng itu,
Dialogue: 0,0:05:23.31,0:05:24.55,btth,,0,0,0,,Sungguh aneh.
Dialogue: 0,0:05:25.31,0:05:25.95,btth,,0,0,0,,Dia,
Dialogue: 0,0:05:26.31,0:05:28.11,btth,,0,0,0,,sepertinya mengenaliku sebagai orang Klan Xiao.
Dialogue: 0,0:05:28.31,0:05:30.23,btth,,0,0,0,,Dia juga pernah menjadi petarung puncak.
Dialogue: 0,0:05:30.47,0:05:32.67,btth,,0,0,0,,Bagaimana mungkin tidak mengenali tanda Klan Xiao-mu?
Dialogue: 0,0:05:32.75,0:05:33.55,btth,,0,0,0,,Apa?
Dialogue: 0,0:05:33.83,0:05:34.63,btth,,0,0,0,,Tanda klan?
Dialogue: 0,0:05:35.07,0:05:36.11,btth,,0,0,0,,Aku punya tanda klan apa?
Dialogue: 0,0:05:36.35,0:05:37.27,btth,,0,0,0,,Kau tidak tahu?
Dialogue: 0,0:05:37.75,0:05:39.31,btth,,0,0,0,,Setiap kali aura-mu meledak,
Dialogue: 0,0:05:39.35,0:05:41.31,btth,,0,0,0,,tanda klan Xiao akan muncul di wajahmu.
Dialogue: 0,0:05:45.35,0:05:46.15,btth,,0,0,0,,Sepertinya,
Dialogue: 0,0:05:46.15,0:05:47.07,btth,,0,0,0,,Sage Pedang Darah itu,
Dialogue: 0,0:05:47.07,0:05:48.95,btth,,0,0,0,,tidak takut pada tanda klan Xiao itu sendiri,
Dialogue: 0,0:05:49.31,0:05:50.95,btth,,0,0,0,,tapi pada orang yang terkait dengan tanda itu.
Dialogue: 0,0:05:51.79,0:05:52.95,btth,,0,0,0,,- Leluhur Xiao Xuan!\N- Senior Xiao Xuan!
Dialogue: 0,0:05:58.15,0:06:00.07,btth,,0,0,0,,Tapi Senior Xiao Xuan sudah lama meninggal,
Dialogue: 0,0:06:00.07,0:06:02.15,btth,,0,0,0,,bagaimana bisa membuat Sage Pedang Darah begitu takut?
Dialogue: 0,0:06:04.15,0:06:05.63,btth,,0,0,0,,Makam Langit sangat unik.
Dialogue: 0,0:06:05.63,0:06:07.51,btth,,0,0,0,,Jika Sage Pedang Darah yang setingkat Ban Sheng,
Dialogue: 0,0:06:07.51,0:06:09.35,btth,,0,0,0,,bisa hidup dengan cara lain,
Dialogue: 0,0:06:09.63,0:06:10.83,btth,,0,0,0,,maka Senior Xiao Xuan,
Dialogue: 0,0:06:10.83,0:06:12.23,btth,,0,0,0,,pasti lebih mudah.
Dialogue: 0,0:06:13.83,0:06:14.83,btth,,0,0,0,,Begitu sampai di tujuan,
Dialogue: 0,0:06:14.83,0:06:16.47,btth,,0,0,0,,semua teka-teki ini akan terungkap.
Dialogue: 0,0:06:28.07,0:06:30.31,btth,,0,0,0,,Kita sekarang sudah memasuki bagian terdalam Makam Langit.
Dialogue: 0,0:06:31.11,0:06:32.31,btth,,0,0,0,,Dengan kecepatan kita,
Dialogue: 0,0:06:32.31,0:06:33.23,btth,,0,0,0,,tak butuh waktu lama,
Dialogue: 0,0:06:33.31,0:06:35.83,btth,,0,0,0,,untuk mencapai makam Xiao Xuan yang tercatat dalam kitab kuno.
Dialogue: 0,0:06:36.35,0:06:37.63,btth,,0,0,0,,Jalannya cukup lancar.
Dialogue: 0,0:06:37.91,0:06:40.23,btth,,0,0,0,,Sage Pedang Darah telah membantu kita.
Dialogue: 0,0:06:43.03,0:06:43.83,btth,,0,0,0,,Xiao Yan,
Dialogue: 0,0:06:44.47,0:06:45.35,btth,,0,0,0,,kecepatan kalian,
Dialogue: 0,0:06:45.35,0:06:46.67,btth,,0,0,0,,terlalu lambat.
Dialogue: 0,0:06:52.11,0:06:52.91,btth,,0,0,0,,Gawat!
Dialogue: 0,0:07:22.35,0:07:23.15,btth,,0,0,0,,Ini...
Dialogue: 0,0:07:23.67,0:07:24.83,btth,,0,0,0,,ruang Klan Hun!
Dialogue: 0,0:07:56.11,0:07:57.31,btth,,0,0,0,,Dou Sheng Klan Hun!
Dialogue: 0,0:07:58.11,0:07:58.91,btth,,0,0,0,,Tak kusangka
Dialogue: 0,0:07:59.15,0:08:00.63,btth,,0,0,0,,tubuh energi Dou Sheng Klan Hun,
Dialogue: 0,0:08:00.63,0:08:02.55,btth,,0,0,0,,bisa menciptakan ruang sendiri di Makam Langit.
Dialogue: 0,0:08:05.23,0:08:06.03,btth,,0,0,0,,Xiao Yan,
Dialogue: 0,0:08:06.31,0:08:08.31,btth,,0,0,0,,kita akhirnya bertemu lagi.
Dialogue: 0,0:08:11.51,0:08:12.47,btth,,0,0,0,,Senior Hun Diao,
Dialogue: 0,0:08:12.71,0:08:15.27,btth,,0,0,0,,orang bernama Xiao Yan itu adalah sisa-sisa Klan Xiao.
Dialogue: 0,0:08:15.71,0:08:18.31,btth,,0,0,0,,Hanya mengandalkan dirinya sendiri, dia mencapai level ini.
Dialogue: 0,0:08:18.31,0:08:21.35,btth,,0,0,0,,Klan Xiao mungkin bangkit kembali di tangannya.
Dialogue: 0,0:08:21.71,0:08:23.03,btth,,0,0,0,,Dia tidak boleh dibiarkan hidup!
Dialogue: 0,0:08:23.31,0:08:25.55,btth,,0,0,0,,Tanpa mengandalkan kekuatan darah?
Dialogue: 0,0:08:25.55,0:08:25.55,btth,,0,0,0,,Tanpa mengandalkan kekuatan darah?
Dialogue: 0,0:08:25.55,0:08:26.03,btth,,0,0,0,,Tanpa mengandalkan kekuatan darah?
Dialogue: 0,0:08:26.23,0:08:28.63,btth,,0,0,0,,Maka dia memang harus dihabisi.
Dialogue: 0,0:08:29.11,0:08:29.95,btth,,0,0,0,,Serang!
Dialogue: 0,0:08:30.43,0:08:31.91,btth,,0,0,0,,Kita terlalu dekat dengan orang itu.
Dialogue: 0,0:08:32.23,0:08:33.83,btth,,0,0,0,,Hati-hati, jangan sampai terlambat.
Dialogue: 0,0:08:36.67,0:08:37.11,btth,,0,0,0,,Pergi!
Dialogue: 0,0:08:41.23,0:08:42.15,btth,,0,0,0,,Bubar! Lari terpisah!
Dialogue: 0,0:08:55.47,0:08:56.51,btth,,0,0,0,,Mereka ini,
Dialogue: 0,0:08:56.51,0:08:59.11,btth,,0,0,0,,adalah petarung top generasi muda Klan Gu.
Dialogue: 0,0:08:59.23,0:09:00.07,btth,,0,0,0,,Bunuh mereka!
Dialogue: 0,0:09:00.07,0:09:02.27,btth,,0,0,0,,Ini akan menjadi pukulan besar bagi Klan Gu.
Dialogue: 0,0:09:02.83,0:09:05.51,btth,,0,0,0,,Kalau begitu bunuh mereka sekalian.
Dialogue: 0,0:09:33.67,0:09:35.43,btth,,0,0,0,,Memang ada sedikit kemampuan.
Dialogue: 0,0:09:35.63,0:09:38.03,btth,,0,0,0,,Tapi ini masih jauh dari cukup.
Dialogue: 0,0:09:42.03,0:09:43.67,btth,,0,0,0,,Skyfire Three Mysterious Change!
Dialogue: 0,0:09:46.51,0:09:47.47,btth,,0,0,0,,Heavenly...
Dialogue: 0,0:09:47.47,0:09:48.83,btth,,0,0,0,,Creation Palm!
Dialogue: 0,0:10:24.11,0:10:25.51,btth,,0,0,0,,Pedang Darah!
Dialogue: 0,0:10:26.95,0:10:28.43,btth,,0,0,0,,Terima kasih atas bantuan Senior.
Dialogue: 0,0:10:28.63,0:10:29.91,btth,,0,0,0,,Bodoh sekali mereka...
Dialogue: 0,0:10:29.91,0:10:31.63,btth,,0,0,0,,berani menyerang Klan Xiao.
Dialogue: 0,0:10:31.87,0:10:33.47,btth,,0,0,0,,Jika orang itu mengetahuinya...
Dialogue: 0,0:10:33.47,0:10:35.51,btth,,0,0,0,,dia pasti akan membantai seluruh Makam Langit.
Dialogue: 0,0:10:36.03,0:10:38.11,btth,,0,0,0,,Tapi aku menyelamatkan anggota klannya...
Dialogue: 0,0:10:38.27,0:10:40.47,btth,,0,0,0,,dia pasti puas dengan tindakanku.
Dialogue: 0,0:10:41.03,0:10:44.67,btth,,0,0,0,,Hanya seorang Ban Sheng, berani bertingkah di hadapan kami berdua.
Dialogue: 0,0:10:45.03,0:10:45.95,btth,,0,0,0,,Cari mati!
Dialogue: 0,0:11:15.31,0:11:16.11,btth,,0,0,0,,Cepat pergi!
Dialogue: 0,0:11:20.75,0:11:21.51,btth,,0,0,0,,Kunci!
Dialogue: 0,0:11:41.31,0:11:44.03,btth,,0,0,0,,Pedang Darah Pemotong Angin!
Dialogue: 0,0:11:52.31,0:11:54.03,btth,,0,0,0,,Hanya segini kemampuannya,
Dialogue: 0,0:11:54.23,0:11:55.87,btth,,0,0,0,,berani ikut campur.
Dialogue: 0,0:12:22.38,0:12:23.71,btth,,0,0,0,,Kenapa orang itu masih belum bertindak?
Dialogue: 0,0:12:51.18,0:12:53.26,btth,,0,0,0,,Tak kusangka di Makam Langit,
Dialogue: 0,0:12:53.50,0:12:55.56,btth,,0,0,0,,masih ada sisa-sisa anggota Klan Hun.
Dialogue: 0,0:12:56.00,0:12:58.00,btth,,0,0,0,,Ternyata dulu aku tidak membunuh semuanya.
Dialogue: 0,0:12:58.00,0:12:59.98,btth,,0,0,0,,Masih ada yang lolos.
Dialogue: 0,0:13:02.82,0:13:03.84,btth,,0,0,0,,Kekuatan ini...
Dialogue: 0,0:13:04.32,0:13:05.20,btth,,0,0,0,,...terasa sangat familiar.
Dialogue: 0,0:13:25.41,0:13:26.41,btth,,0,0,0,,Klan Hun...
Dialogue: 0,0:13:26.69,0:13:30.09,btth,,0,0,0,,bau darahmu benar-benar menjijikkan.
Dialogue: 0,0:13:32.81,0:13:34.09,btth,,0,0,0,,Xiao Xuan!
Dialogue: 0,0:13:40.21,0:13:41.01,btth,,0,0,0,,Leluhur!
Dialogue: 0,0:13:42.49,0:13:43.89,btth,,0,0,0,,Senior Xiao Xuan!
Dialogue: 0,0:13:45.01,0:13:45.81,btth,,0,0,0,,Tuan Xiao Xuan!
Dialogue: 0,0:13:45.89,0:13:46.73,btth,,0,0,0,,Anda datang!
Dialogue: 0,0:13:52.21,0:13:53.21,btth,,0,0,0,,Senior Hun Diao...
Dialogue: 0,0:13:53.29,0:13:54.01,btth,,0,0,0,,Ini...
Dialogue: 0,0:13:56.21,0:13:58.53,btth,,0,0,0,,Meski Xiao Xuan sangat kuat,
Dialogue: 0,0:13:58.61,0:14:00.61,btth,,0,0,0,,dia tetap ditekan oleh Makam Langit.
Dialogue: 0,0:14:00.89,0:14:03.29,btth,,0,0,0,,Hanya bisa keluar dari makam sekali setiap 10 tahun.
Dialogue: 0,0:14:03.41,0:14:05.53,btth,,0,0,0,,Batas 10 tahun belum tercapai,
Dialogue: 0,0:14:05.61,0:14:07.49,btth,,0,0,0,,mustahil dia muncul dalam wujud asli.
Dialogue: 0,0:14:08.01,0:14:08.81,btth,,0,0,0,,Benar!
Dialogue: 0,0:14:09.01,0:14:10.29,btth,,0,0,0,,Yang di depan kita...
Dialogue: 0,0:14:10.29,0:14:11.81,btth,,0,0,0,,...hanya bayangan semu.
Dialogue: 0,0:14:13.01,0:14:14.53,btth,,0,0,0,,Kau cukup tajam melihatnya.
Dialogue: 0,0:14:14.81,0:14:16.69,btth,,0,0,0,,Sayangnya terlalu menganggap remeh.
Dialogue: 0,0:14:17.21,0:14:18.89,btth,,0,0,0,,Dua Dou Sheng bintang 1,
Dialogue: 0,0:14:19.01,0:14:22.01,btth,,0,0,0,,tidak pantas berbicara seenaknya di hadapanku.
Dialogue: 0,0:14:54.73,0:14:55.61,btth,,0,0,0,,Tuan itu,
Dialogue: 0,0:14:55.81,0:14:56.81,btth,,0,0,0,,sudah bertindak lagi.
Dialogue: 0,0:15:00.73,0:15:01.49,btth,,0,0,0,,Lari cepat!
Dialogue: 0,0:15:26.01,0:15:26.73,btth,,0,0,0,,Ini...
Dialogue: 0,0:15:31.73,0:15:32.89,btth,,0,0,0,,Kekuatan penghancurnya...
Dialogue: 0,0:15:33.53,0:15:35.21,btth,,0,0,0,,Leluhur memang benar-benar kejam.
Dialogue: 0,0:15:35.81,0:15:38.13,btth,,0,0,0,,Orang berbahaya itu masih kejam seperti biasa.
Dialogue: 0,0:15:39.01,0:15:40.01,btth,,0,0,0,,Klan Xiao...
Dialogue: 0,0:15:40.81,0:15:42.53,btth,,0,0,0,,akhirnya ada yang datang.
Dialogue: 0,0:15:43.01,0:15:44.89,btth,,0,0,0,,Tidak sia-sia aku menunggu bertahun-tahun.
Dialogue: 0,0:15:47.81,0:15:49.09,btth,,0,0,0,,Keturunan tak berguna Xiao Yan,
Dialogue: 0,0:15:49.49,0:15:50.49,btth,,0,0,0,,memberi salam pada leluhur.
Dialogue: 0,0:15:52.09,0:15:53.01,btth,,0,0,0,,Xiao Yan...
Dialogue: 0,0:15:53.21,0:15:54.69,btth,,0,0,0,,Nama yang bagus.
Dialogue: 0,0:15:56.53,0:15:57.92,btth,,0,0,0,,Sejak kau memasuki Makam Langit
Dialogue: 0,0:15:58.13,0:15:59.49,btth,,0,0,0,,aku sudah merasakan kehadiranmu.
Dialogue: 0,0:15:59.69,0:16:01.29,btth,,0,0,0,,Perilakumu sepanjang perjalanan,
Dialogue: 0,0:16:01.69,0:16:02.49,btth,,0,0,0,,cukup baik.
Dialogue: 0,0:16:02.93,0:16:04.01,btth,,0,0,0,,Junior Gu Xun'er,
Dialogue: 0,0:16:04.01,0:16:05.33,btth,,0,0,0,,memberi salam pada Senior Xiao Xuan.
Dialogue: 0,0:16:05.41,0:16:06.33,btth,,0,0,0,,Junior Klan Gu,
Dialogue: 0,0:16:06.41,0:16:07.53,btth,,0,0,0,,memberi salam pada Senior Xiao Xuan.
Dialogue: 0,0:16:08.81,0:16:10.49,btth,,0,0,0,,Junior dari Klan Gu ya.
Dialogue: 0,0:16:11.09,0:16:12.29,btth,,0,0,0,,Kalian dari Klan Gu,
Dialogue: 0,0:16:12.41,0:16:15.61,btth,,0,0,0,,tidak sepenuhnya mematuhi perjanjian yang kita buat dulu.
Dialogue: 0,0:16:16.41,0:16:17.33,btth,,0,0,0,,Senior Xiao Xuan,
Dialogue: 0,0:16:17.41,0:16:19.01,btth,,0,0,0,,itu hanya sebagian kecil anggota klan.
Dialogue: 0,0:16:19.23,0:16:21.57,btth,,0,0,0,,Ketua klan selalu mematuhi perjanjian.
Dialogue: 0,0:16:23.41,0:16:24.61,btth,,0,0,0,,Garis Darahmu...
Dialogue: 0,0:16:24.73,0:16:26.41,btth,,0,0,0,,mirip dengan Gu Yuan.
Dialogue: 0,0:16:31.21,0:16:33.13,btth,,0,0,0,,Gu Yuan adalah ayahku.
Dialogue: 0,0:16:33.49,0:16:35.41,btth,,0,0,0,,Jadi kau putri orang itu.
Dialogue: 0,0:16:35.89,0:16:37.89,btth,,0,0,0,,Pantasan memiliki garis darah Tingkat Dewa.
Dialogue: 0,0:16:38.01,0:16:40.81,btth,,0,0,0,,Benar-benar melebihi gurunya.
Dialogue: 0,0:16:41.89,0:16:43.09,btth,,0,0,0,,Kau boleh pulang dulu.
Dialogue: 0,0:16:43.21,0:16:44.01,btth,,0,0,0,,Mulai sekarang...
Dialogue: 0,0:16:44.29,0:16:46.69,btth,,0,0,0,,...kau bisa tetap tinggal di Makam Langit.
Dialogue: 0,0:16:47.81,0:16:49.21,btth,,0,0,0,,Terima kasih Tuan Xiao Xuan.
Dialogue: 0,0:16:55.21,0:16:56.93,btth,,0,0,0,,Karena pengaruh Makam Langit,
Dialogue: 0,0:16:57.21,0:16:59.33,btth,,0,0,0,,wujud asliku tidak bisa meninggalkan makam.
Dialogue: 0,0:16:59.41,0:17:01.13,btth,,0,0,0,,Yang kalian lihat sekarang...
Dialogue: 0,0:17:01.29,0:17:04.41,btth,,0,0,0,,...hanyalah proyeksi dariku.
Dialogue: 0,0:17:05.13,0:17:06.29,btth,,0,0,0,,Untuk hari ini...
Dialogue: 0,0:17:06.69,0:17:08.41,btth,,0,0,0,,aku juga sudah menunggu terlalu lama.
Dialogue: 0,0:17:09.13,0:17:11.33,btth,,0,0,0,,Kau pasti punya banyak pertanyaan.
Dialogue: 0,0:17:11.49,0:17:12.89,btth,,0,0,0,,Ikut aku ke makam dulu.
Dialogue: 0,0:17:25.41,0:17:26.01,btth,,0,0,0,,Baik.
Dialogue: 0,0:17:26.21,0:17:27.21,btth,,0,0,0,,Kita sampai.
Dialogue: 0,0:17:29.21,0:17:32.21,btth,,0,0,0,,Ini tempat dengan energi paling pekat di Makam Langit.
Dialogue: 0,0:17:32.61,0:17:34.93,btth,,0,0,0,,Kalian bisa berlatih di sini.
Dialogue: 0,0:17:35.41,0:17:37.49,btth,,0,0,0,,Xiao Yan dan gadis Klan Gu itu...
Dialogue: 0,0:17:37.89,0:17:39.01,btth,,0,0,0,,...ikut aku masuk.
Dialogue: 0,0:17:44.29,0:17:45.09,btth,,0,0,0,,Xiao Yan,
Dialogue: 0,0:17:45.49,0:17:46.49,btth,,0,0,0,,jaga baik-baik Xun'er.
Dialogue: 0,0:17:46.81,0:17:47.61,btth,,0,0,0,,Tenang.
Dialogue: 0,0:17:48.21,0:17:49.01,btth,,0,0,0,,Ayo kita pergi.
Dialogue: 0,0:18:16.01,0:18:19.69,btth,,0,0,0,,Bisakah kau ceritakan keadaan Klan Xiao sekarang?
Dialogue: 0,0:18:21.01,0:18:23.01,btth,,0,0,0,,Klan Xiao sudah tidak ada lagi.
Dialogue: 0,0:18:23.69,0:18:25.53,btth,,0,0,0,,Yang tersisa hanyalah keluarga Xiao yang merosot.
Dialogue: 0,0:18:26.41,0:18:28.21,btth,,0,0,0,,Sejak garis darah Klan Xiao habis,
Dialogue: 0,0:18:28.81,0:18:30.61,btth,,0,0,0,,bakat pelatihan anggota Klan Xiao,
Dialogue: 0,0:18:30.61,0:18:32.09,btth,,0,0,0,,juga semakin melemah.
Dialogue: 0,0:18:32.61,0:18:35.41,btth,,0,0,0,,Sulit menemukan orang yang bisa disebut jenius di dalam klan.
Dialogue: 0,0:18:41.41,0:18:43.41,btth,,0,0,0,,Yang lebih menjengkelkan adalah orang-orang Klan Hun.
Dialogue: 0,0:18:44.41,0:18:46.53,btth,,0,0,0,,Mereka tidak hanya menyiksa anggota keluarga Xiao,
Dialogue: 0,0:18:47.21,0:18:50.09,btth,,0,0,0,,tetapi juga menculik ayahku.
Dialogue: 0,0:18:50.81,0:18:51.69,btth,,0,0,0,,Kakak Xiao Yan.
Dialogue: 0,0:18:52.61,0:18:54.41,btth,,0,0,0,,Aku sudah menduga Klan Xiao akan merosot seperti ini.
Dialogue: 0,0:18:55.01,0:18:56.49,btth,,0,0,0,,Aku sudah memprediksinya.
Dialogue: 0,0:18:57.21,0:18:59.29,btth,,0,0,0,,Saat Klan Xiao masih ada,
Dialogue: 0,0:18:59.61,0:19:03.09,btth,,0,0,0,,aku sudah merasakan darah Dou Di Klan Xiao akan segera habis.
Dialogue: 0,0:19:03.53,0:19:06.13,btth,,0,0,0,,Satu-satunya cara untuk mengisi kembali darah Dou Di,
Dialogue: 0,0:19:06.41,0:19:08.81,btth,,0,0,0,,adalah munculnya Dou Di baru di dalam klan.
Dialogue: 0,0:19:10.21,0:19:12.41,btth,,0,0,0,,Dulu aku sangat sombong.
Dialogue: 0,0:19:12.89,0:19:15.33,btth,,0,0,0,,Setelah berdiskusi dengan banyak tetua klan,
Dialogue: 0,0:19:15.53,0:19:17.49,btth,,0,0,0,,kami memutuskan untuk mengorbankan segalanya.
Dialogue: 0,0:19:18.21,0:19:19.61,btth,,0,0,0,,Kami menggunakan teknik rahasia,
Dialogue: 0,0:19:19.81,0:19:21.73,btth,,0,0,0,,untuk memindahkan seluruh kekuatan garis darah klan,
Dialogue: 0,0:19:21.73,0:19:23.69,btth,,0,0,0,,ke dalam tubuhku.
Dialogue: 0,0:19:24.53,0:19:25.41,btth,,0,0,0,,Aku mengerti,
Dialogue: 0,0:19:26.01,0:19:28.41,btth,,0,0,0,,itu adalah harapan terakhir seluruh klan.
Dialogue: 0,0:19:29.01,0:19:30.01,btth,,0,0,0,,Sayangnya,
Dialogue: 0,0:19:30.21,0:19:32.01,btth,,0,0,0,,aku tetap gagal.
Dialogue: 0,0:19:32.21,0:19:34.29,btth,,0,0,0,,Dan setelah gagal mencapai Dou Di,
Dialogue: 0,0:19:34.53,0:19:36.33,btth,,0,0,0,,aku diserang diam-diam oleh Klan Hun,
Dialogue: 0,0:19:37.01,0:19:38.33,btth,,0,0,0,,luka parah dan mati.
Dialogue: 0,0:19:41.69,0:19:43.01,btth,,0,0,0,,Itu adalah cara terakhir.
Dialogue: 0,0:19:43.41,0:19:45.01,btth,,0,0,0,,Jika membiarkan kekuatan darah habis,
Dialogue: 0,0:19:45.21,0:19:46.89,btth,,0,0,0,,Klan Xiao juga tidak akan selamat.
Dialogue: 0,0:19:47.69,0:19:48.49,btth,,0,0,0,,Klan Xiao,
Dialogue: 0,0:19:49.01,0:19:50.29,btth,,0,0,0,,tidak akan ada yang menyalahkanmu.
Dialogue: 0,0:19:52.41,0:19:54.89,btth,,0,0,0,,Tapi jika kekuatan darah Klan Xiao sudah habis,
Dialogue: 0,0:19:55.21,0:19:57.29,btth,,0,0,0,,mengapa aku memiliki tanda klan?
Dialogue: 0,0:19:57.89,0:19:59.49,btth,,0,0,0,,Karena tanda klan Klan Xiao,
Dialogue: 0,0:19:59.61,0:20:01.21,btth,,0,0,0,,bukan diberikan oleh orang lain,
Dialogue: 0,0:20:01.21,0:20:03.21,btth,,0,0,0,,melainkan diperoleh melalui pelatihan.
Dialogue: 0,0:20:05.41,0:20:06.61,btth,,0,0,0,,Aku merasakan,
Dialogue: 0,0:20:06.89,0:20:09.61,btth,,0,0,0,,aura Skyfire Three Mysterious Change padamu.
Dialogue: 0,0:20:10.13,0:20:10.93,btth,,0,0,0,,Dan aku,
Dialogue: 0,0:20:11.41,0:20:13.33,btth,,0,0,0,,adalah penciptanya.
Dialogue: 0,0:20:14.01,0:20:14.81,btth,,0,0,0,,Xiao Xuan.
Dialogue: 0,0:20:15.61,0:20:17.33,btth,,0,0,0,,Jadi Xiao Xuan yang disebut Ketua Tang,
Dialogue: 0,0:20:17.41,0:20:18.61,btth,,0,0,0,,adalah leluhur.
Dialogue: 0,0:20:20.21,0:20:22.53,btth,,0,0,0,,Tapi Skyfire Three Mysterious Change yang kau pelajari,
Dialogue: 0,0:20:22.69,0:20:24.21,btth,,0,0,0,,masih kurang satu langkah terakhir,
Dialogue: 0,0:20:24.29,0:20:26.53,btth,,0,0,0,,sehingga kau belum bisa menciptakan tanda klan yang sebenarnya.
Dialogue: 0,0:20:26.89,0:20:29.01,btth,,0,0,0,,Dan tanpa dukungan kekuatan darah,
Dialogue: 0,0:20:29.21,0:20:32.21,btth,,0,0,0,,sulit untuk mengoptimalkan kekuatan tanda klan.
Dialogue: 0,0:20:32.81,0:20:34.89,btth,,0,0,0,,Untungnya dulu aku mengerahkan seluruh tenaga,
Dialogue: 0,0:20:35.01,0:20:36.89,btth,,0,0,0,,untuk menyimpan sebagian darah Dou Di.
Dialogue: 0,0:20:37.21,0:20:38.81,btth,,0,0,0,,Dalam kekuatan darah ini,
Dialogue: 0,0:20:38.89,0:20:41.49,btth,,0,0,0,,terdapat energi yang kusegel semasa hidupku.
Dialogue: 0,0:20:43.01,0:20:44.09,btth,,0,0,0,,Sekarang ini,
Dialogue: 0,0:20:44.41,0:20:46.21,btth,,0,0,0,,aku hanyalah sisa jiwa,
Dialogue: 0,0:20:46.61,0:20:48.89,btth,,0,0,0,,yang hanya bisa melayang di Makam Langit.
Dialogue: 0,0:20:50.01,0:20:52.01,btth,,0,0,0,,Tapi langit tidak menghancurkan Klan Xiao,
Dialogue: 0,0:20:52.53,0:20:53.49,btth,,0,0,0,,aku percaya,
Dialogue: 0,0:20:54.21,0:20:55.89,btth,,0,0,0,,kau akan melakukannya lebih baik dariku.
