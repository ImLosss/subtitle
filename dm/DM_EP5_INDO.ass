Dialogue: 0,0:00:07.53,0:00:09.13,dm,,0,0,0,,Ling'er berdoa kepada para dewa,
Dialogue: 0,0:00:09.37,0:00:11.03,dm,,0,0,0,,semoga kakak selalu sehat,
Dialogue: 0,0:00:11.33,0:00:12.43,dm,,0,0,0,,dan semuanya lancar.
Dialogue: 0,0:00:29.97,0:00:30.70,dm,,0,0,0,,Ling'er.
Dialogue: 0,0:01:02.93,0:01:03.93,dm,,0,0,0,,Ling'er.
Dialogue: 0,0:01:04.47,0:01:05.47,dm,,0,0,0,,Ling'er.
Dialogue: 0,0:01:07.13,0:01:08.43,dm,,0,0,0,,Dilihat dari kondisinya,
Dialogue: 0,0:01:08.53,0:01:09.97,dm,,0,0,0,,ini pasti energi jahat.
Dialogue: 0,0:01:10.13,0:01:10.93,dm,,0,0,0,,Kakak,
Dialogue: 0,0:01:10.93,0:01:12.33,dm,,0,0,0,,mari kita coba gunakan Formasi Penenang Jiwa,
Dialogue: 0,0:01:12.43,0:01:14.23,dm,,0,0,0,,untuk sementara menekan energi jahat ini.
Dialogue: 0,0:01:14.93,0:01:16.23,dm,,0,0,0,,Itu ide yang bagus.
Dialogue: 0,0:01:16.23,0:01:18.03,dm,,0,0,0,,Langit dan bumi berpusat pada Xuanwu,
Dialogue: 0,0:01:18.23,0:01:19.83,dm,,0,0,0,,mengusir kejahatan dan menenangkan empat penjuru.
Dialogue: 0,0:01:20.20,0:01:21.80,dm,,0,0,0,,Tenangkan jiwa dan pikiran, tenang!
Dialogue: 0,0:01:26.63,0:01:27.43,dm,,0,0,0,,Kakak Ding,
Dialogue: 0,0:01:27.53,0:01:29.83,dm,,0,0,0,,kenapa Ling'er bisa terkena energi jahat seberat ini?
Dialogue: 0,0:01:29.83,0:01:30.53,dm,,0,0,0,,Pagi ini,
Dialogue: 0,0:01:30.63,0:01:32.03,dm,,0,0,0,,Ling'er sedang berdoa di tepi sungai,
Dialogue: 0,0:01:32.23,0:01:34.33,dm,,0,0,0,,tak sengaja jatuh ke air sungai yang terkontaminasi energi jahat.
Dialogue: 0,0:01:35.07,0:01:35.77,dm,,0,0,0,,Kudengar,
Dialogue: 0,0:01:35.77,0:01:37.17,dm,,0,0,0,,ada Roh Jahat yang berkeliaran di Sungai Bai,
Dialogue: 0,0:01:37.17,0:01:38.37,dm,,0,0,0,,sedang menyebabkan wabah.
Dialogue: 0,0:01:38.43,0:01:40.23,dm,,0,0,0,,Sungai ini terhubung dengan Sungai Bai,
Dialogue: 0,0:01:40.43,0:01:41.13,dm,,0,0,0,,mungkinkah...
Dialogue: 0,0:01:41.13,0:01:42.23,dm,,0,0,0,,Roh Jahat yang berulah?
Dialogue: 0,0:01:42.63,0:01:45.83,dm,,0,0,0,,Sebelumnya di Desa Zhao juga seperti ini.
Dialogue: 0,0:01:47.23,0:01:48.03,dm,,0,0,0,,Berhasil.
Dialogue: 0,0:01:57.33,0:01:58.13,dm,,0,0,0,,Kak,
Dialogue: 0,0:01:58.47,0:01:59.27,dm,,0,0,0,,Ling'er,
Dialogue: 0,0:01:59.43,0:02:00.23,dm,,0,0,0,,kau sudah bangun?
Dialogue: 0,0:02:01.10,0:02:01.63,dm,,0,0,0,,Jangan takut,
Dialogue: 0,0:02:01.63,0:02:03.33,dm,,0,0,0,,kakak di sini.
Dialogue: 0,0:02:03.93,0:02:04.73,dm,,0,0,0,,Adik Ding,
Dialogue: 0,0:02:05.13,0:02:06.23,dm,,0,0,0,,aku perlu bicara denganmu.
Dialogue: 0,0:02:07.93,0:02:08.63,dm,,0,0,0,,Sebelumnya,
Dialogue: 0,0:02:08.63,0:02:10.23,dm,,0,0,0,,jimat yang di Desa Zhao itu,
Dialogue: 0,0:02:10.43,0:02:11.63,dm,,0,0,0,,aku tidak bermaksud menyembunyikannya.
Dialogue: 0,0:02:12.03,0:02:13.20,dm,,0,0,0,,Boleh aku lihat sekali lagi?
Dialogue: 0,0:02:13.27,0:02:13.83,dm,,0,0,0,,Baik.
Dialogue: 0,0:02:14.83,0:02:15.83,dm,,0,0,0,,Tidak salah lagi,
Dialogue: 0,0:02:16.03,0:02:18.23,dm,,0,0,0,,ini jimat dari Kota Luofeng lima belas tahun lalu.
Dialogue: 0,0:02:18.53,0:02:19.87,dm,,0,0,0,,Sejak kejadian itu,
Dialogue: 0,0:02:20.33,0:02:22.63,dm,,0,0,0,,jimat jenis ini menghilang tanpa jejak.
Dialogue: 0,0:02:22.97,0:02:24.07,dm,,0,0,0,,Lima belas tahun lalu,
Dialogue: 0,0:02:24.73,0:02:26.33,dm,,0,0,0,,Kakak Liu tahu sesuatu?
Dialogue: 0,0:02:27.13,0:02:27.93,dm,,0,0,0,,Maaf,
Dialogue: 0,0:02:28.13,0:02:29.03,dm,,0,0,0,,beberapa hal,
Dialogue: 0,0:02:29.23,0:02:30.83,dm,,0,0,0,,sungguh tidak bisa kuingat.
Dialogue: 0,0:02:32.23,0:02:33.93,dm,,0,0,0,,Tentang energi jahat ini,
Dialogue: 0,0:02:34.23,0:02:35.97,dm,,0,0,0,,aku punya satu cara.
Dialogue: 0,0:02:36.63,0:02:39.73,dm,,0,0,0,,Energi jahat ini adalah benda yang sangat Yin dan dingin,
Dialogue: 0,0:02:39.73,0:02:40.47,dm,,0,0,0,,Kalian bilang,
Dialogue: 0,0:02:40.57,0:02:42.63,dm,,0,0,0,,energi jahat seperti ini paling takut apa?
Dialogue: 0,0:02:43.97,0:02:44.97,dm,,0,0,0,,Energi jahat,
Dialogue: 0,0:02:45.37,0:02:47.07,dm,,0,0,0,,paling takut dengan benda Yang murni.
Dialogue: 0,0:02:47.93,0:02:48.87,dm,,0,0,0,,Benar sekali!
Dialogue: 0,0:02:49.33,0:02:51.43,dm,,0,0,0,,Jika kita bisa mendapatkan Pil Emas Praktisi Roh,
Dialogue: 0,0:02:51.47,0:02:52.23,dm,,0,0,0,,pasti bisa!
Dialogue: 0,0:02:52.57,0:02:53.33,dm,,0,0,0,,Iya!
Dialogue: 0,0:02:53.77,0:02:55.53,dm,,0,0,0,,Benda ini bisa membantu orang menembus batas,
Dialogue: 0,0:02:55.77,0:02:57.03,dm,,0,0,0,,adalah benda Yang murni,
Dialogue: 0,0:02:57.37,0:02:59.53,dm,,0,0,0,,pasti punya efek menekan energi jahat.
Dialogue: 0,0:02:59.67,0:03:00.43,dm,,0,0,0,,Monyet,
Dialogue: 0,0:03:01.17,0:03:02.43,dm,,0,0,0,,pikiranmu cepat sekali.
Dialogue: 0,0:03:03.47,0:03:04.23,dm,,0,0,0,,Tapi,
Dialogue: 0,0:03:04.27,0:03:05.57,dm,,0,0,0,,untuk mendapatkan benda ini,
Dialogue: 0,0:03:05.57,0:03:07.83,dm,,0,0,0,,harus ikut Turnamen Pemusnahan Roh Jahat tiga hari lagi.
Dialogue: 0,0:03:08.07,0:03:09.03,dm,,0,0,0,,Tapi untuk mendapatkannya,
Dialogue: 0,0:03:09.07,0:03:10.23,dm,,0,0,0,,tidak mudah!
Dialogue: 0,0:03:10.47,0:03:11.23,dm,,0,0,0,,Aku ikut!
Dialogue: 0,0:03:14.07,0:03:14.87,dm,,0,0,0,,Adik Ding,
Dialogue: 0,0:03:15.37,0:03:17.83,dm,,0,0,0,,pendaftaran berakhir hari ini saat senja.
Dialogue: 0,0:03:17.97,0:03:19.83,dm,,0,0,0,,Jika kau ingin memperebutkan Pil Emas Praktisi Roh itu,
Dialogue: 0,0:03:20.07,0:03:21.03,dm,,0,0,0,,cepat daftar!
Dialogue: 0,0:03:21.07,0:03:22.43,dm,,0,0,0,,Aku akan cari Komandan Ling!
Dialogue: 0,0:03:26.07,0:03:26.83,dm,,0,0,0,,Mana orangnya?
Dialogue: 0,0:03:27.77,0:03:28.57,dm,,0,0,0,,Terlambat?
Dialogue: 0,0:03:35.27,0:03:36.03,dm,,0,0,0,,Nona Bai,
Dialogue: 0,0:03:36.37,0:03:37.13,dm,,0,0,0,,kenapa?
Dialogue: 0,0:03:41.57,0:03:42.33,dm,,0,0,0,,Ding Xiao,
Dialogue: 0,0:03:42.77,0:03:44.03,dm,,0,0,0,,tunjukkan Manifestasi Roh-mu!
Dialogue: 0,0:03:45.03,0:04:00.03,dm,,2,2,0,,Join channel telegram @dongworld agar tidak ketinggalan update.\NDonghua ini diterjemahkan oleh Dongworld.
Dialogue: 0,0:04:09.33,0:04:10.30,dm,,0,0,0,,Berhenti di sini.
Dialogue: 0,0:04:12.33,0:04:13.13,dm,,0,0,0,,Nona Bai,
Dialogue: 0,0:04:13.83,0:04:15.63,dm,,0,0,0,,sudah dapat jawaban yang kau cari?
Dialogue: 0,0:04:16.13,0:04:16.93,dm,,0,0,0,,Jawaban?
Dialogue: 0,0:04:17.43,0:04:18.23,dm,,0,0,0,,Jawaban apa?
Dialogue: 0,0:04:19.67,0:04:20.23,dm,,0,0,0,,Oh iya,
Dialogue: 0,0:04:20.23,0:04:21.03,dm,,0,0,0,,Komandan Ling,
Dialogue: 0,0:04:21.23,0:04:23.43,dm,,0,0,0,,aku mau daftar ikut Turnamen Pemusnahan Roh Jahat lusa.
Dialogue: 0,0:04:23.73,0:04:24.53,dm,,0,0,0,,Ding Xiao,
Dialogue: 0,0:04:25.03,0:04:26.43,dm,,0,0,0,,tak kusangka dalam waktu singkat,
Dialogue: 0,0:04:26.93,0:04:29.13,dm,,0,0,0,,kau sudah mencapai level enam Murid Roh.
Dialogue: 0,0:04:29.53,0:04:32.37,dm,,0,0,0,,Dan tadi kau bisa menggunakan lima jimat sekaligus.
Dialogue: 0,0:04:33.03,0:04:33.87,dm,,0,0,0,,Aku setuju.
Dialogue: 0,0:04:34.23,0:04:35.07,dm,,0,0,0,,Terima kasih Komandan Ling.
Dialogue: 0,0:04:36.07,0:04:36.87,dm,,0,0,0,,Nona Bai,
Dialogue: 0,0:04:37.17,0:04:39.03,dm,,0,0,0,,kau sudah memaksa Ding Xiao sedemikian rupa,
Dialogue: 0,0:04:39.63,0:04:41.23,dm,,0,0,0,,tapi dia tetap tidak bisa menunjukkan Manifestasi Roh.
Dialogue: 0,0:04:41.93,0:04:44.83,dm,,0,0,0,,Kedepannya jangan menyusahkannya lagi.
Dialogue: 0,0:04:46.53,0:04:47.93,dm,,0,0,0,,Aku masih ada urusan,
Dialogue: 0,0:04:48.57,0:04:49.67,dm,,0,0,0,,tidak bisa menemani kalian.
Dialogue: 0,0:04:50.53,0:04:51.57,dm,,0,0,0,,Hormat pada Komandan Ling.
Dialogue: 0,0:04:53.13,0:04:53.93,dm,,0,0,0,,Ding Xiao,
Dialogue: 0,0:04:54.83,0:04:56.03,dm,,0,0,0,,aku akan membawamu ke suatu tempat.
Dialogue: 0,0:04:56.63,0:04:57.43,dm,,0,0,0,,Ikut aku.
Dialogue: 0,0:05:02.33,0:05:03.33,dm,,0,0,0,,Tadi maaf,
Dialogue: 0,0:05:03.93,0:05:04.83,dm,,0,0,0,,kau baik-baik saja?
Dialogue: 0,0:05:05.73,0:05:06.53,dm,,0,0,0,,Tidak apa-apa.
Dialogue: 0,0:05:07.57,0:05:08.37,dm,,0,0,0,,Ding Xiao,
Dialogue: 0,0:05:08.73,0:05:10.43,dm,,0,0,0,,kenapa kau memiliki Manifestasi Abnormal?
Dialogue: 0,0:05:11.03,0:05:11.83,dm,,0,0,0,,Nona Bai,
Dialogue: 0,0:05:12.13,0:05:13.83,dm,,0,0,0,,kenapa kau terus terobsesi dengan hal ini?
Dialogue: 0,0:05:15.23,0:05:18.87,dm,,0,0,0,,Ibuku menghilang di Kota Luofeng lima belas tahun lalu.
Dialogue: 0,0:05:20.03,0:05:22.23,dm,,0,0,0,,Aku pernah kembali ke Kota Luofeng mencari petunjuk,
Dialogue: 0,0:05:22.53,0:05:23.73,dm,,0,0,0,,tapi tidak menemukan apa-apa.
Dialogue: 0,0:05:24.03,0:05:27.57,dm,,0,0,0,,Sampai jimat tua itu ditemukan di Desa Zhao sebelumnya.
Dialogue: 0,0:05:28.03,0:05:30.93,dm,,0,0,0,,Jimat tua ini juga muncul di kuil tua.
Dialogue: 0,0:05:32.43,0:05:33.63,dm,,0,0,0,,Kakak Liu memberitahuku,
Dialogue: 0,0:05:33.97,0:05:35.43,dm,,0,0,0,,itu adalah jimat tua Kota Luofeng,
Dialogue: 0,0:05:36.03,0:05:37.57,dm,,0,0,0,,teknik pembuatannya sudah hilang.
Dialogue: 0,0:05:38.13,0:05:40.03,dm,,0,0,0,,Aku juga mencarinya di perpustakaan,
Dialogue: 0,0:05:41.13,0:05:45.23,dm,,0,0,0,,dan jimat ini juga terlibat dengan organisasi misterius bernama Gerbang Jiuyou.
Dialogue: 0,0:05:45.53,0:05:46.63,dm,,0,0,0,,Jimat ini terus muncul,
Dialogue: 0,0:05:46.63,0:05:47.63,dm,,0,0,0,,pasti bukan kebetulan.
Dialogue: 0,0:05:48.23,0:05:50.43,dm,,0,0,0,,Jika aku bisa menyelidiki hal di balik jimat ini,
Dialogue: 0,0:05:50.70,0:05:51.43,dm,,0,0,0,,mungkin,
Dialogue: 0,0:05:52.03,0:05:53.23,dm,,0,0,0,,mungkin aku bisa menemukan,
Dialogue: 0,0:05:53.73,0:05:55.73,dm,,0,0,0,,alasan ibuku menghilang.
Dialogue: 0,0:05:56.67,0:05:58.27,dm,,0,0,0,,Selama ini aku selalu yakin,
Dialogue: 0,0:05:58.63,0:05:59.97,dm,,0,0,0,,selama aku bertahan,
Dialogue: 0,0:06:00.93,0:06:02.43,dm,,0,0,0,,pasti bisa menemukan ibu.
Dialogue: 0,0:06:03.83,0:06:06.43,dm,,0,0,0,,Ternyata kamu sedang mencari ibumu.
Dialogue: 0,0:06:08.83,0:06:09.63,dm,,0,0,0,,Nona Bai,
Dialogue: 0,0:06:10.33,0:06:12.03,dm,,0,0,0,,jika ada yang bisa kubantu,
Dialogue: 0,0:06:12.77,0:06:14.13,dm,,0,0,0,,aku akan berusaha sekuat tenaga.
Dialogue: 0,0:06:18.67,0:06:19.47,dm,,0,0,0,,Terima kasih.
Dialogue: 0,0:06:20.83,0:06:21.63,dm,,0,0,0,,Ding Xiao,
Dialogue: 0,0:06:22.23,0:06:24.47,dm,,0,0,0,,dalam tragedi Kota Luofeng lima belas tahun lalu,
Dialogue: 0,0:06:24.77,0:06:26.87,dm,,0,0,0,,hanya sepasang bayi laki-laki dan perempuan yang diselamatkan.
Dialogue: 0,0:06:27.33,0:06:28.37,dm,,0,0,0,,Dihitung waktunya,
Dialogue: 0,0:06:28.83,0:06:30.37,dm,,0,0,0,,sekarang hampir seusia denganmu,
Dialogue: 0,0:06:30.43,0:06:31.23,dm,,0,0,0,,Maksudmu,
Dialogue: 0,0:06:32.17,0:06:34.27,dm,,0,0,0,,aku dan Ling'er adalah bayi laki-laki dan perempuan itu?
Dialogue: 0,0:06:36.64,0:06:37.73,dm,,0,0,0,,Mungkin.
Dialogue: 0,0:06:37.97,0:06:39.37,dm,,0,0,0,,Ditambah sebelumnya di kuil tua,
Dialogue: 0,0:06:39.73,0:06:41.50,dm,,0,0,0,,kau bisa menyerap Laba-laba Beracun Bermata Enam.
Dialogue: 0,0:06:42.93,0:06:45.33,dm,,0,0,0,,Ding Xiao,
Dialogue: 0,0:06:45.37,0:06:46.23,dm,,0,0,0,,kau baik-baik saja?
Dialogue: 0,0:06:48.57,0:06:49.43,dm,,0,0,0,,Sudah terlalu lama,
Dialogue: 0,0:06:50.53,0:06:52.43,dm,,0,0,0,,aku sudah tidak ingat apa-apa.
Dialogue: 0,0:06:55.77,0:06:56.63,dm,,0,0,0,,Tidak apa-apa.
Dialogue: 0,0:06:56.97,0:06:58.47,dm,,0,0,0,,Kau sudah banyak membantuku.
Dialogue: 0,0:06:59.77,0:07:00.63,dm,,0,0,0,,Aku pamit.
Dialogue: 0,0:07:02.13,0:07:02.83,dm,,0,0,0,,Nona Bai.
Dialogue: 0,0:07:07.33,0:07:08.83,dm,,0,0,0,,Jangan panggil saya Nona,
Dialogue: 0,0:07:09.13,0:07:10.33,dm,,0,0,0,,panggil saja Bai Xi.
Dialogue: 0,0:07:30.73,0:07:34.33,dm,,0,0,0,,Sudah lama tidak mencium bau manusia.
Dialogue: 0,0:07:38.03,0:07:39.37,dm,,0,0,0,,Lima belas tahun,
Dialogue: 0,0:07:39.93,0:07:40.73,dm,,0,0,0,,kita,
Dialogue: 0,0:07:41.03,0:07:42.53,dm,,0,0,0,,bertemu lagi.
Dialogue: 0,0:07:42.83,0:07:43.63,dm,,0,0,0,,Kau?
Dialogue: 0,0:07:45.93,0:07:46.77,dm,,0,0,0,,Ini dimana?
Dialogue: 0,0:07:47.57,0:07:48.63,dm,,0,0,0,,Keluarkan aku!
Dialogue: 0,0:08:05.43,0:08:06.20,dm,,0,0,0,,Kak Ding,
Dialogue: 0,0:08:06.63,0:08:08.13,dm,,0,0,0,,ini Menara Penunduk Roh.
Dialogue: 0,0:08:09.03,0:08:14.48,dm,,300,300,0,,Di dalam menara ini, disegel roh-roh jahat yang telah mencelakai dunia sejak pertama kali Divisi Penunduk Roh didirikan.
Dialogue: 0,0:08:14.83,0:08:15.90,dm,,0,0,0,,Ujian kali ini,
Dialogue: 0,0:08:16.03,0:08:21.40,dm,,300,300,0,,tim yang membunuh cukup banyak Roh Jahat akan mendapat kualifikasi masuk Divisi Gui, Kabupaten Longlin.
Dialogue: 0,0:08:21.53,0:08:22.70,dm,,0,0,0,,Sampai lantai lima,
Dialogue: 0,0:08:22.83,0:08:26.83,dm,,0,0,0,,dan tim dengan total Kekuatan Manifestasi tertinggi akan menjadi juara pertama.
Dialogue: 0,0:08:27.03,0:08:28.90,dm,,0,0,0,,Bisa masuk perpustakaan divisi kami,
Dialogue: 0,0:08:29.23,0:08:30.10,dm,,0,0,0,,ditambah hadiah,
Dialogue: 0,0:08:30.93,0:08:32.30,dm,,0,0,0,,Pil Emas Praktisi Roh.
Dialogue: 0,0:08:32.53,0:08:34.10,dm,,0,0,0,,Benar ada Pil Emas Praktisi Roh.
Dialogue: 0,0:08:34.13,0:08:35.03,dm,,0,0,0,,Ini harta langka.
Dialogue: 0,0:08:35.03,0:08:35.97,dm,,0,0,0,,Pil ini harus kudapatkan.
Dialogue: 0,0:08:35.97,0:08:37.07,dm,,0,0,0,,Asal menang langsung dapat.
Dialogue: 0,0:08:37.93,0:08:40.50,dm,,0,0,0,,Jimat ini adalah Jimat Pemakan Roh,
Dialogue: 0,0:08:40.93,0:08:42.70,dm,,0,0,0,,bisa membawa kalian masuk menara,
Dialogue: 0,0:08:42.83,0:08:45.10,dm,,0,0,0,,dan mencatat Kekuatan Manifestasi serangan kalian.
Dialogue: 0,0:08:45.73,0:08:47.63,dm,,0,0,0,,Menara Penunduk Roh punya lima lantai.
Dialogue: 0,0:08:48.03,0:08:48.80,dm,,0,0,0,,Ingat,
Dialogue: 0,0:08:49.13,0:08:52.30,dm,,0,0,0,,semakin tinggi lantai, level Roh Jahat semakin tinggi.
Dialogue: 0,0:08:52.63,0:08:54.50,dm,,0,0,0,,Roh Jahat di lantai lima adalah,
Dialogue: 0,0:08:55.03,0:08:57.70,dm,,0,0,0,,Makhluk Berkepala Burung.
Dialogue: 0,0:08:58.53,0:08:59.23,dm,,0,0,0,,Roh Jahat level Raja.
Dialogue: 0,0:09:01.13,0:09:02.80,dm,,0,0,0,,Saat ini aku hanya Murid Roh level enam,
Dialogue: 0,0:09:03.13,0:09:06.60,dm,,0,0,0,,jauh 13 level dari Pejuang Roh level satu.
Dialogue: 0,0:09:07.53,0:09:08.30,dm,,0,0,0,,Tenang!
Dialogue: 0,0:09:10.63,0:09:14.63,dm,,0,0,0,,Makhluk Berkepala Burung sudah dikunci 12 rantai Langit,
Dialogue: 0,0:09:15.70,0:09:17.93,dm,,0,0,0,,hanya bisa bergerak dalam radius 10 meter.
Dialogue: 0,0:09:18.03,0:09:21.93,dm,,0,0,0,,Kekuatannya juga dibatasi Jimat Penangkal Roh ke level Murid Roh tertinggi.
Dialogue: 0,0:09:22.03,0:09:23.70,dm,,0,0,0,,Kalian tidak perlu terlalu khawatir.
Dialogue: 0,0:09:23.73,0:09:24.80,dm,,0,0,0,,Kalau begitu,
Dialogue: 0,0:09:25.43,0:09:26.50,dm,,0,0,0,,bisa dicoba.
Dialogue: 0,0:09:26.93,0:09:29.13,dm,,0,0,0,,Semua murid Divisi Penunduk Roh dengarkan perintah,
Dialogue: 0,0:09:30.13,0:09:30.90,dm,,0,0,0,,masuk menara!
Dialogue: 0,0:09:46.33,0:09:47.90,dm,,0,0,0,,Hati-hati, Roh Jahat datang.
Dialogue: 0,0:09:49.03,0:09:49.90,dm,,0,0,0,,Semuanya waspada.
Dialogue: 0,0:09:51.37,0:09:52.03,dm,,0,0,0,,Tunggu sebentar,
Dialogue: 0,0:09:52.37,0:09:53.17,dm,,0,0,0,,biar aku yang urus.
Dialogue: 0,0:09:53.93,0:09:55.53,dm,,0,0,0,,Asal ada Kakek Monyet di sini,
Dialogue: 0,0:09:55.63,0:09:58.60,dm,,0,0,0,,Roh Jahat ini jangan harap bisa melukai kalian sedikitpun.
Dialogue: 0,0:09:58.63,0:10:00.40,dm,,0,0,0,,Aku bisa lawan sepuluh!
Dialogue: 0,0:10:17.90,0:10:19.40,dm,,0,0,0,,Di depan ada formasi teleportasi.
Dialogue: 0,0:10:21.70,0:10:23.10,dm,,0,0,0,,Benar-benar seperti monyet.
Dialogue: 0,0:10:23.43,0:10:25.60,dm,,0,0,0,,Aku sedang mengintai jalan untuk kalian.
Dialogue: 0,0:10:33.30,0:10:34.13,dm,,0,0,0,,Senior, hati-hati!
Dialogue: 0,0:10:37.17,0:10:38.00,dm,,0,0,0,,Kau baik-baik saja?
Dialogue: 0,0:10:41.50,0:10:42.70,dm,,0,0,0,,Ini baru lantai dua,
Dialogue: 0,0:10:43.23,0:10:44.90,dm,,0,0,0,,mereka sudah tidak sabar.
Dialogue: 0,0:10:47.80,0:10:48.60,dm,,0,0,0,,Kak Ding,
Dialogue: 0,0:10:48.70,0:10:49.60,dm,,0,0,0,,Roh Jahat menutup pintu keluar.
Dialogue: 0,0:10:50.90,0:10:51.73,dm,,0,0,0,,Tidak bisa masuk.
Dialogue: 0,0:10:51.90,0:10:53.40,dm,,0,0,0,,Monyet, giliranmu tampil.
Dialogue: 0,0:10:55.20,0:10:56.30,dm,,0,0,0,,Roh Jahat busuk,
Dialogue: 0,0:10:56.30,0:10:57.53,dm,,0,0,0,,aku di sini!
Dialogue: 0,0:10:57.90,0:10:58.90,dm,,0,0,0,,Berani datang sini?
Dialogue: 0,0:10:59.13,0:10:59.90,dm,,0,0,0,,Ayo, ayo, ayo!
Dialogue: 0,0:11:04.70,0:11:06.00,dm,,0,0,0,,Semuanya, serang sekarang!
Dialogue: 0,0:11:06.23,0:11:08.00,dm,,0,0,0,,Pengunci Berat, kunci!
Dialogue: 0,0:11:27.80,0:11:31.03,dm,,0,0,0,,Jimat level rendah ini bisa menghasilkan kekuatan sebesar ini?
Dialogue: 0,0:11:31.90,0:11:34.40,dm,,0,0,0,,Teknik jimat Adik Ding sangat ahli.
Dialogue: 0,0:11:35.00,0:11:36.10,dm,,0,0,0,,Aku akan melindungi kalian.
Dialogue: 0,0:11:36.40,0:11:38.10,dm,,0,0,0,,Kita harus sampai di lantai lima lebih dulu.
Dialogue: 0,0:11:40.60,0:11:44.23,dm,,0,0,0,,Ding Xiao itu ternyata menahan semua serangan Roh Jahat sendirian.
Dialogue: 0,0:11:44.43,0:11:46.30,dm,,0,0,0,,Tidakkah dia khawatir Kekuatan Manifestasinya habis terlalu cepat?
Dialogue: 0,0:11:46.80,0:11:47.60,dm,,0,0,0,,Yu Xuan,
Dialogue: 0,0:11:47.90,0:11:49.40,dm,,0,0,0,,aku temukan formasi ke lantai tiga.
Dialogue: 0,0:11:49.40,0:11:50.20,dm,,0,0,0,,Cepat pergi.
Dialogue: 0,0:11:58.07,0:11:59.03,dm,,0,0,0,,Senior Miao, cepat!
Dialogue: 0,0:11:59.07,0:11:59.67,dm,,0,0,0,,Senior!
Dialogue: 0,0:12:11.30,0:12:12.93,dm,,0,0,0,,Kekuatan Roh Jahat semakin kuat.
Dialogue: 0,0:12:13.30,0:12:14.37,dm,,0,0,0,,Semuanya harus berhati-hati.
Dialogue: 0,0:12:18.50,0:12:19.70,dm,,0,0,0,,Satu jam telah berlalu,
Dialogue: 0,0:12:20.10,0:12:21.63,dm,,0,0,0,,masih ada tiga tim di dalam.
Dialogue: 0,0:12:22.20,0:12:23.00,dm,,0,0,0,,Tidak seru,
Dialogue: 0,0:12:23.50,0:12:25.23,dm,,0,0,0,,kupikir bisa melihat pertandingan.
Dialogue: 0,0:12:25.50,0:12:27.43,dm,,0,0,0,,Roh Jahat di Menara Penunduk Roh sangat berbahaya,
Dialogue: 0,0:12:28.10,0:12:29.93,dm,,0,0,0,,orang luar jangan sembarangan masuk.
Dialogue: 0,0:12:30.23,0:12:31.33,dm,,0,0,0,,Mereka ingin mencapai puncak,
Dialogue: 0,0:12:31.89,0:12:33.33,dm,,0,0,0,,tidak hanya butuh Kekuatan Manifestasi yang cukup,
Dialogue: 0,0:12:33.90,0:12:35.63,dm,,0,0,0,,tapi juga strategi dan keberanian.
Dialogue: 0,0:12:38.20,0:12:38.90,dm,,0,0,0,,Adik,
Dialogue: 0,0:12:38.90,0:12:39.80,dm,,0,0,0,,tolong, selamatkan aku.
Dialogue: 0,0:12:45.00,0:12:46.10,dm,,0,0,0,,Kekuatan Manifestasiku habis.
Dialogue: 0,0:12:46.70,0:12:47.70,dm,,0,0,0,,Aku tamat.
Dialogue: 0,0:12:48.10,0:12:50.33,dm,,0,0,0,,Keluarkan aku!
Dialogue: 0,0:12:51.33,0:13:06.33,dm,,2,2,0,,Join channel telegram @dongworld agar tidak ketinggalan update.\NDonghua ini diterjemahkan oleh Dongworld.
Dialogue: 0,0:13:23.90,0:13:25.50,dm,,0,0,0,,Kumpulkan Roh Menjadi Wujud, muncul!
Dialogue: 0,0:13:27.20,0:13:29.07,dm,,0,0,0,,Pedang Terbang, hancur!
Dialogue: 0,0:13:30.07,0:13:45.07,dm,,2,2,0,,Join channel telegram @dongworld agar tidak ketinggalan update.\NDonghua ini diterjemahkan oleh Dongworld.
Dialogue: 0,0:14:14.63,0:14:16.33,dm,,0,0,0,,Kalian benar-benar menghabiskan semua kekuatan manifestasi?
Dialogue: 0,0:14:16.80,0:14:18.50,dm,,0,0,0,,Tidak menyisakan sedikit pun untuk mundur?
Dialogue: 0,0:14:19.40,0:14:20.20,dm,,0,0,0,,Kak Ding,
Dialogue: 0,0:14:20.70,0:14:21.50,dm,,0,0,0,,kenapa?
Dialogue: 0,0:14:21.80,0:14:23.50,dm,,0,0,0,,Menggunakan Manifestasi disini tidak akan dihitung.
Dialogue: 0,0:14:23.50,0:14:24.50,dm,,0,0,0,,Kalian naik dulu,
Dialogue: 0,0:14:24.70,0:14:25.70,dm,,0,0,0,,aku menyusul.
Dialogue: 0,0:14:26.40,0:14:27.20,dm,,0,0,0,,Cepat pergi!
Dialogue: 0,0:14:27.60,0:14:28.73,dm,,0,0,0,,Roh Jahat di sini ganas,
Dialogue: 0,0:14:29.20,0:14:30.93,dm,,0,0,0,,Jimat Penyelamat hanya bisa melindungi kalian sesaat.
Dialogue: 0,0:14:36.50,0:14:37.73,dm,,0,0,0,,Waktu sudah hampir habis,
Dialogue: 0,0:14:38.63,0:14:40.93,dm,,0,0,0,,masih banyak murid bertahan di menara.
Dialogue: 0,0:14:41.80,0:14:44.50,dm,,0,0,0,,Generasi muda Divisi Penunduk Roh mengagumkan.
Dialogue: 0,0:14:46.20,0:14:47.40,dm,,0,0,0,,Ding Xiao belum keluar.
Dialogue: 0,0:14:48.30,0:14:50.80,dm,,0,0,0,,Ternyata dia benar-benar meningkat banyak belakangan ini.
Dialogue: 0,0:14:51.80,0:14:52.33,dm,,0,0,0,,Ling'er,
Dialogue: 0,0:14:52.33,0:14:54.64,dm,,0,0,0,,aku sudah sampai lantai lima.
Dialogue: 0,0:14:55.23,0:14:56.67,dm,,0,0,0,,Asal melewati tahap ini,
Dialogue: 0,0:14:56.67,0:14:57.90,dm,,0,0,0,,bisa mendapatkan Pil Praktisi Roh.
Dialogue: 0,0:15:01.70,0:15:02.90,dm,,0,0,0,,Ini?
Dialogue: 0,0:15:08.33,0:15:08.77,dm,,0,0,0,,Cepat lari!
Dialogue: 0,0:15:10.37,0:15:11.17,dm,,0,0,0,,Bentuk formasi!
Dialogue: 0,0:15:11.47,0:15:12.77,dm,,0,0,0,,Kunci Menara Penunduk Roh!
Dialogue: 0,0:15:13.10,0:15:14.60,dm,,0,0,0,,Murid Divisi Gui masih di dalam menara.
Dialogue: 0,0:15:15.17,0:15:16.47,dm,,0,0,0,,Kau tahu sendiri,
Dialogue: 0,0:15:16.77,0:15:18.97,dm,,0,0,0,,Murid Roh level enam melawan Roh Jahat level Raja,
Dialogue: 0,0:15:19.60,0:15:21.10,dm,,0,0,0,,tidak mungkin selamat.
Dialogue: 0,0:15:24.00,0:15:24.77,dm,,0,0,0,,Bahaya!
Dialogue: 0,0:15:24.77,0:15:25.57,dm,,0,0,0,,Cepat menghindar!
Dialogue: 0,0:15:28.73,0:15:29.53,dm,,0,0,0,,Ding Xiao,
Dialogue: 0,0:15:30.37,0:15:31.30,dm,,0,0,0,,harus selamat.
