Dialogue: 0,0:15:03.43,0:15:07.69,komen,,0,0,0,,{\pos(187,516)\fad(500,250)}[Kota Pingyang\NDesa Pinggiran]
Dialogue: 0,0:00:00.00,0:00:01.00,tomb,,1,1,0,,Join channel telegram @dongworld agar tidak ketinggalan update.\NDonghua ini diterjemahkan oleh Dongworld.
Dialogue: 0,0:00:24.43,0:00:26.50,tomb,,0,0,0,,Kenapa belum sampai di Kota Pingyang?
Dialogue: 0,0:00:26.70,0:00:28.37,tomb,,0,0,0,,Kakek ini mau minum.
Dialogue: 0,0:00:28.73,0:00:29.43,tomb,,0,0,0,,Air.
Dialogue: 0,0:00:30.10,0:00:31.27,tomb,,0,0,0,,Sama makanan.
Dialogue: 0,0:00:36.73,0:00:37.70,tomb,,0,0,0,,Sudah cukup.
Dialogue: 0,0:00:38.27,0:00:39.87,tomb,,0,0,0,,Kurasa kau sudah lama menyerap semua energi spiritual.
Dialogue: 0,0:00:40.13,0:00:41.13,tomb,,0,0,0,,Di sini malas-malasan dan pura-pura.
Dialogue: 0,0:00:42.10,0:00:42.63,tomb,,0,0,0,,Apa?
Dialogue: 0,0:00:45.17,0:00:45.73,tomb,,0,0,0,,Aduh.
Dialogue: 0,0:00:45.73,0:00:47.23,tomb,,0,0,0,,Demi langit dan bumi.
Dialogue: 0,0:00:47.37,0:00:48.27,tomb,,0,0,0,,Ada yang menyiksa naga.
Dialogue: 0,0:00:48.50,0:00:50.23,tomb,,0,0,0,,Menyiksa naga, tolong!
Dialogue: 0,0:00:55.33,0:00:55.77,tomb,,0,0,0,,Long Wu.
Dialogue: 0,0:00:56.23,0:00:56.87,tomb,,0,0,0,,Kamu mau makan apa?
Dialogue: 0,0:00:59.73,0:01:00.23,tomb,,0,0,0,,Long Wu.
Dialogue: 0,0:01:11.53,0:01:12.23,tomb,,0,0,0,,Hmph.
Dialogue: 0,0:01:12.77,0:01:13.23,tomb,,0,0,0,,Oh.
Dialogue: 0,0:01:13.70,0:01:14.55,tomb,,0,0,0,,Dihindari?
Dialogue: 0,0:01:15.37,0:01:17.97,tomb,,0,0,0,,Giok Teratai memang pantas disebut harta karun Taois tertinggi.
Dialogue: 0,0:01:18.67,0:01:21.17,tomb,,0,0,0,,Jalan Kekacauan di darat brutal dan kejam.
Dialogue: 0,0:01:21.77,0:01:23.30,tomb,,0,0,0,,Tapi tak bisa mengalahkan harta terbang.
Dialogue: 0,0:01:23.77,0:01:24.63,tomb,,0,0,0,,Kau juga tahu.
Dialogue: 0,0:01:25.07,0:01:26.70,tomb,,0,0,0,,Ribu tahun lalu, Raja Iblis sekte kalian,
Dialogue: 0,0:01:26.90,0:01:27.97,tomb,,0,0,0,,bagaimana kalah telak.
Dialogue: 0,0:01:28.37,0:01:29.13,tomb,,0,0,0,,Kau tak bisa mengalahkanku.
Dialogue: 0,0:01:30.97,0:01:31.83,tomb,,0,0,0,,Ya.
Dialogue: 0,0:01:32.67,0:01:34.63,tomb,,0,0,0,,Sudah seribu tahun.
Dialogue: 0,0:01:35.73,0:01:37.13,tomb,,0,0,0,,Hahaha.
Dialogue: 0,0:01:37.53,0:01:38.67,tomb,,0,0,0,,Menurutmu.
Dialogue: 0,0:01:39.30,0:01:40.37,tomb,,0,0,0,,Jalan Kekacauan.
Dialogue: 0,0:01:41.10,0:01:44.53,tomb,,0,0,0,,tak bisa menciptakan artefak terbang sendiri?
Dialogue: 0,0:01:49.63,0:01:51.23,tomb,,0,0,0,,Kegagalan legendaris itu,
Dialogue: 0,0:01:51.83,0:01:53.27,tomb,,0,0,0,,ternyata berhasil kalian ciptakan.
Dialogue: 0,0:01:57.33,0:01:59.07,tomb,,0,0,0,,Hahaha.
Dialogue: 0,0:01:59.57,0:02:02.27,tomb,,0,0,0,,Harta karun ajaib yang menyatukan Tao dan sihir,
Dialogue: 0,0:02:02.73,0:02:04.37,tomb,,0,0,0,,ditambah ilmu bela diri.
Dialogue: 0,0:02:05.17,0:02:06.97,tomb,,0,0,0,,khusus menandingi Giok Teratai sekte kalian.
Dialogue: 0,0:02:55.97,0:02:58.63,tomb,,0,0,0,,Jalan Kekacauan ternyata memperbaiki Sayap Dewa Iblis.
Dialogue: 0,0:02:59.47,0:03:01.30,tomb,,0,0,0,,Memang di luar perkiraanku.
Dialogue: 0,0:03:02.30,0:03:17.30,tomb,,1,1,0,,Join channel telegram @dongworld agar tidak ketinggalan update.\NDonghua ini diterjemahkan oleh Dongworld.
Dialogue: 0,0:03:29.67,0:03:30.90,tomb,,0,0,0,,Kalau begitu,
Dialogue: 0,0:03:31.47,0:03:33.03,tomb,,0,0,0,,meski harus membayar harga,
Dialogue: 0,0:03:33.67,0:03:35.30,tomb,,0,0,0,,hari ini aku akan membunuhmu.
Dialogue: 0,0:03:36.87,0:03:38.77,tomb,,0,0,0,,Lawan yang kuat,
Dialogue: 0,0:03:39.53,0:03:42.33,tomb,,0,0,0,,memang lebih memicu keinginan untuk menaklukkan.
Dialogue: 0,0:03:48.13,0:03:48.70,tomb,,0,0,0,,Long Wu.
Dialogue: 0,0:03:50.57,0:03:51.03,tomb,,0,0,0,,Long Wu.
Dialogue: 0,0:03:58.63,0:03:59.23,tomb,,0,0,0,,Masuklah ke dalam.
Dialogue: 0,0:03:59.77,0:04:00.27,tomb,,0,0,0,,Jangan sampai kedinginan.
Dialogue: 0,0:04:00.92,0:04:01.45,tomb,,0,0,0,,Tidak.
Dialogue: 0,0:04:02.30,0:04:03.50,tomb,,0,0,0,,Aku ingin di luar sebentar.
Dialogue: 0,0:04:04.67,0:04:06.23,tomb,,0,0,0,,Perasaan kehilangan orang yang penting.
Dialogue: 0,0:04:07.13,0:04:07.97,tomb,,0,0,0,,Aku juga sangat mengerti.
Dialogue: 0,0:04:09.30,0:04:10.27,tomb,,0,0,0,,Saat masih kecil,
Dialogue: 0,0:04:10.93,0:04:11.97,tomb,,0,0,0,,di musim dingin seperti ini juga,
Dialogue: 0,0:04:12.73,0:04:15.23,tomb,,0,0,0,,Kakak keempat dikirim ayah untuk belajar pada seorang master.
Dialogue: 0,0:04:16.03,0:04:17.03,tomb,,0,0,0,,Hari perpisahan itu,
Dialogue: 0,0:04:17.43,0:04:18.43,tomb,,0,0,0,,aku menangis sangat keras,
Dialogue: 0,0:04:19.03,0:04:20.27,tomb,,0,0,0,,mengira tak akan bertemu lagi.
Dialogue: 0,0:04:21.27,0:04:22.10,tomb,,0,0,0,,Saat aku besar,
Dialogue: 0,0:04:22.33,0:04:23.03,tomb,,0,0,0,,dia kembali,
Dialogue: 0,0:04:23.83,0:04:25.43,tomb,,0,0,0,,kultivasinya juga tak terbaca lagi.
Dialogue: 0,0:04:26.53,0:04:27.43,tomb,,0,0,0,,Bertahun-tahun tak bertemu,
Dialogue: 0,0:04:27.90,0:04:28.97,tomb,,0,0,0,,tapi perasaan kami,
Dialogue: 0,0:04:29.23,0:04:30.73,tomb,,0,0,0,,masih sama baiknya seperti saat kecil.
Dialogue: 0,0:04:31.30,0:04:32.57,tomb,,0,0,0,,Aku dan Qian Long cocok,
Dialogue: 0,0:04:33.07,0:04:34.23,tomb,,0,0,0,,sayangnya belum sempat berteman,
Dialogue: 0,0:04:34.43,0:04:35.13,tomb,,0,0,0,,tapi sudah berpisah selamanya.
Dialogue: 0,0:04:38.23,0:04:39.50,tomb,,0,0,0,,Kakak selalu seperti ini,
Dialogue: 0,0:04:40.43,0:04:41.90,tomb,,0,0,0,,dia selalu yang paling menonjol,
Dialogue: 0,0:04:42.30,0:04:43.33,tomb,,0,0,0,,di antara teman sebayanya.
Dialogue: 0,0:04:44.13,0:04:45.10,tomb,,0,0,0,,Dia adalah kakakku,
Dialogue: 0,0:04:45.63,0:04:47.07,tomb,,0,0,0,,keluarga yang paling baik padaku.
Dialogue: 0,0:04:48.27,0:04:49.17,tomb,,0,0,0,,Tapi sekarang dia...
Dialogue: 0,0:04:50.37,0:04:50.57,tomb,,0,0,0,,Dia...
Dialogue: 0,0:04:50.57,0:04:51.27,tomb,,0,0,0,,Menangislah.
Dialogue: 0,0:04:53.10,0:04:53.93,tomb,,0,0,0,,Aku tak bisa melupakannya,
Dialogue: 0,0:04:59.53,0:05:00.57,tomb,,0,0,0,,Kalau tak bisa ya sudah,
Dialogue: 0,0:05:01.23,0:05:02.70,tomb,,0,0,0,,Ikatan masa lalu sangat berharga,
Dialogue: 0,0:05:03.73,0:05:06.27,tomb,,0,0,0,,tapi waktu juga akan memberimu ikatan baru.
Dialogue: 0,0:05:07.70,0:05:09.13,tomb,,0,0,0,,Terima kasih telah menemaniku sepanjang jalan,
Dialogue: 0,0:05:09.47,0:05:10.87,tomb,,0,0,0,,dan mendengarkanku bercerita banyak.
Dialogue: 0,0:05:11.73,0:05:12.57,tomb,,0,0,0,,Antara teman,
Dialogue: 0,0:05:12.97,0:05:13.50,tomb,,0,0,0,,ngapain sungkan.
Dialogue: 0,0:05:14.27,0:05:15.33,tomb,,0,0,0,,Jika di sepanjang jalan ini,
Dialogue: 0,0:05:15.43,0:05:17.03,tomb,,0,0,0,,tak ada seorang pun yang menemaniku,
Dialogue: 0,0:05:17.73,0:05:18.33,tomb,,0,0,0,,mungkin,
Dialogue: 0,0:05:19.23,0:05:20.53,tomb,,0,0,0,,aku sudah tak sanggup bertahan.
Dialogue: 0,0:05:24.37,0:05:24.93,tomb,,0,0,0,,Ada yang bertarung.
Dialogue: 0,0:05:26.07,0:05:27.63,tomb,,0,0,0,,Kalian berdua romantis sekali,
Dialogue: 0,0:05:27.63,0:05:29.03,tomb,,0,0,0,,menikmati bulan di salju.
Dialogue: 0,0:05:29.53,0:05:30.50,tomb,,0,0,0,,Kapan,
Dialogue: 0,0:05:30.50,0:05:33.63,tomb,,0,0,0,,aku bisa romantis dengan naga betina?
Dialogue: 0,0:05:41.50,0:05:41.93,tomb,,0,0,0,,Kau mau apa?
Dialogue: 0,0:05:57.67,0:05:58.43,tomb,,0,0,0,,Cepat lihat, cepat lihat!
Dialogue: 0,0:05:58.77,0:05:59.87,tomb,,0,0,0,,Itu si gadis itu!
Dialogue: 0,0:06:00.30,0:06:01.17,tomb,,0,0,0,,Meng Ke'er!
Dialogue: 0,0:06:01.87,0:06:02.23,tomb,,0,0,0,,Belut Tua!
Dialogue: 0,0:06:02.57,0:06:03.47,tomb,,0,0,0,,Cepat bawa aku ke depan untuk melihat!
Dialogue: 0,0:06:03.47,0:06:05.33,tomb,,0,0,0,,Mau menyuruhku lagi?
Dialogue: 0,0:06:05.57,0:06:06.23,tomb,,0,0,0,,Tidak mungkin!
Dialogue: 0,0:06:09.23,0:06:10.23,tomb,,0,0,0,,Aku takut padamu.
Dialogue: 0,0:06:20.23,0:06:22.17,tomb,,0,0,0,,Ternyata hubunganmu dengan Meng Ke'er seperti itu.
Dialogue: 0,0:06:24.07,0:06:25.10,tomb,,0,0,0,,Juga rumit dan sulit dimengerti.
Dialogue: 0,0:06:25.77,0:06:26.93,tomb,,0,0,0,,Tapi sekarang tampaknya,
Dialogue: 0,0:06:27.53,0:06:28.63,tomb,,0,0,0,,masalah yang dihadapinya,
Dialogue: 0,0:06:29.03,0:06:29.90,tomb,,0,0,0,,tidak kalah banyak dariku.
Dialogue: 0,0:06:39.73,0:06:40.70,tomb,,0,0,0,,Sial!
Dialogue: 0,0:06:41.57,0:06:43.57,tomb,,0,0,0,,Ini adalah pertarungan pertamaku setelah keluar,
Dialogue: 0,0:06:44.47,0:06:45.70,tomb,,0,0,0,,tidak boleh gagal.
Dialogue: 0,0:07:00.47,0:07:03.67,tomb,,0,0,0,,Gadis kecil ini kultivasinya meningkat lagi.
Dialogue: 0,0:07:03.97,0:07:05.70,tomb,,0,0,0,,Orang lain itu juga begitu kuat.
Dialogue: 0,0:07:06.33,0:07:08.37,tomb,,0,0,0,,Ini benar-benar pertarungan puncak generasi muda.
Dialogue: 0,0:07:29.73,0:07:30.90,tomb,,0,0,0,,Tombak Fangtian itu,
Dialogue: 0,0:07:31.57,0:07:32.90,tomb,,0,0,0,,benar-benar senjata dewa.
Dialogue: 0,0:07:41.47,0:07:42.63,tomb,,0,0,0,,Tombak dewa miliknya ini,
Dialogue: 0,0:07:43.03,0:07:43.93,tomb,,0,0,0,,ada hubungannya denganku.
Dialogue: 0,0:07:44.37,0:07:45.07,tomb,,0,0,0,,Kau mau...
Dialogue: 0,0:07:46.63,0:07:47.77,tomb,,0,0,0,,Kau, bocah,
Dialogue: 0,0:07:47.90,0:07:49.70,tomb,,0,0,0,,sungguh mewarisi jiwa sejati kami.
Dialogue: 0,0:07:55.67,0:07:56.70,tomb,,0,0,0,,Dia hampir tak sanggup lagi.
Dialogue: 0,0:07:57.57,0:07:59.27,tomb,,0,0,0,,Jika aku membuka segel sepenuhnya,
Dialogue: 0,0:07:59.90,0:08:01.23,tomb,,0,0,0,,mungkin bisa menghilangkan ancaman besar.
Dialogue: 0,0:08:17.57,0:08:17.97,tomb,,0,0,0,,Siapa?
Dialogue: 0,0:08:24.17,0:08:25.63,tomb,,0,0,0,,Baru berpisah beberapa hari,
Dialogue: 0,0:08:26.10,0:08:27.97,tomb,,0,0,0,,gadis kecil sudah tidak mengenaliku.
Dialogue: 0,0:08:29.10,0:08:29.83,tomb,,0,0,0,,Naga bajingan!
Dialogue: 0,0:08:38.07,0:08:38.57,tomb,,0,0,0,,Chen Nan.
Dialogue: 0,0:08:39.33,0:08:40.63,tomb,,0,0,0,,Aku datang di waktu yang tepat.
Dialogue: 0,0:08:40.97,0:08:41.50,tomb,,0,0,0,,Saudara,
Dialogue: 0,0:08:41.93,0:08:42.57,tomb,,0,0,0,,tahan sebentar lagi.
Dialogue: 0,0:08:42.97,0:08:44.07,tomb,,0,0,0,,Yang lain akan segera sampai.
Dialogue: 0,0:08:44.50,0:08:45.13,tomb,,0,0,0,,Yang lain?
Dialogue: 0,0:08:52.23,0:08:53.07,tomb,,0,0,0,,Kau siapa lagi?
Dialogue: 0,0:08:53.93,0:08:55.50,tomb,,0,0,0,,Musuh dari musuh adalah teman.
Dialogue: 0,0:08:56.10,0:08:57.13,tomb,,0,0,0,,Hari ini kita bekerja sama,
Dialogue: 0,0:08:57.57,0:08:58.50,tomb,,0,0,0,,pasti bisa menangkapnya.
Dialogue: 0,0:08:59.23,0:08:59.73,tomb,,0,0,0,,Apa?
Dialogue: 0,0:09:00.47,0:09:00.83,tomb,,0,0,0,,Kalian...
Dialogue: 0,0:09:01.57,0:09:01.90,tomb,,0,0,0,,Chen Nan,
Dialogue: 0,0:09:02.57,0:09:03.63,tomb,,0,0,0,,kau malah bekerja sama dengan Jalan Sesat?
Dialogue: 0,0:09:09.10,0:09:10.10,tomb,,0,0,0,,Chen Nan datang dengan persiapan.
Dialogue: 0,0:09:10.73,0:09:11.83,tomb,,0,0,0,,Di bawah juga ada yang menyergap.
Dialogue: 0,0:09:12.43,0:09:12.70,tomb,,0,0,0,,Tidak bisa,
Dialogue: 0,0:09:13.23,0:09:13.70,tomb,,0,0,0,,tidak bisa melawan langsung.
Dialogue: 0,0:09:15.07,0:09:16.63,tomb,,0,0,0,,Harus cepat ke istana mencari Chu Yue.
Dialogue: 0,0:09:17.37,0:09:18.90,tomb,,0,0,0,,Minta dia membantuku menekan dampak balik segel.
Dialogue: 0,0:09:19.97,0:09:20.43,tomb,,0,0,0,,Ah,
Dialogue: 0,0:09:20.63,0:09:21.17,tomb,,0,0,0,,dia kabur.
Dialogue: 0,0:09:29.27,0:09:29.97,tomb,,0,0,0,,Aku Chen Nan.
Dialogue: 0,0:09:32.07,0:09:33.70,tomb,,0,0,0,,Xiang Tian dari Jalan Kekacauan.
Dialogue: 0,0:09:34.43,0:09:36.87,tomb,,0,0,0,,Kau adalah Pejuang Pelindung Negara Dongchu,
Dialogue: 0,0:09:37.57,0:09:38.33,tomb,,0,0,0,,Chen Nan?
Dialogue: 0,0:09:39.13,0:09:39.47,tomb,,0,0,0,,benar.
Dialogue: 0,0:09:40.03,0:09:40.30,tomb,,0,0,0,,Itu aku.
Dialogue: 0,0:09:41.98,0:09:42.99,tomb,,0,0,0,,Oh.
Dialogue: 0,0:09:53.80,0:09:55.10,tomb,,0,0,0,,Sudah tahu kau pengkhianat,
Dialogue: 0,0:09:55.57,0:09:56.33,tomb,,0,0,0,,sudah waspada padamu.
Dialogue: 0,0:09:58.57,0:10:00.97,tomb,,0,0,0,,Seorang sahabat pernah memintaku,
Dialogue: 0,0:10:01.53,0:10:02.90,tomb,,0,0,0,,jika bertemu denganmu,
Dialogue: 0,0:10:03.23,0:10:05.00,tomb,,0,0,0,,harus membunuhmu.
Dialogue: 0,0:10:06.00,0:10:07.13,tomb,,0,0,0,,Putra Ling Zixu,
Dialogue: 0,0:10:07.77,0:10:08.90,tomb,,0,0,0,,murid Jalan Pemusnah.
Dialogue: 0,0:10:09.17,0:10:11.13,tomb,,0,0,0,,Sudah tahu sebab akibatnya,
Dialogue: 0,0:10:11.73,0:10:13.33,tomb,,0,0,0,,tak perlu kujelaskan lagi.
Dialogue: 0,0:10:16.20,0:10:17.53,tomb,,0,0,0,,Meski aku terluka parah,
Dialogue: 0,0:10:18.03,0:10:19.20,tomb,,0,0,0,,tapi kultivasimu,
Dialogue: 0,0:10:19.77,0:10:21.23,tomb,,0,0,0,,jauh di bawah wanita itu.
Dialogue: 0,0:10:21.90,0:10:22.97,tomb,,0,0,0,,Membunuhmu,
Dialogue: 0,0:10:23.63,0:10:24.50,tomb,,0,0,0,,sudah cukup.
Dialogue: 0,0:10:25.03,0:10:25.60,tomb,,0,0,0,,Benarkah?
Dialogue: 0,0:10:26.60,0:10:41.60,tomb,,1,1,0,,Join channel telegram @dongworld agar tidak ketinggalan update.\NDonghua ini diterjemahkan oleh Dongworld.
Dialogue: 0,0:10:52.53,0:10:53.70,tomb,,0,0,0,,Kalau begitu,
Dialogue: 0,0:10:56.40,0:10:59.10,tomb,,0,0,0,,aku bisa merebut Tombak Fangtian ini dengan terang-terangan.
Dialogue: 0,0:11:01.10,0:11:01.73,tomb,,0,0,0,,Apa?
Dialogue: 0,0:11:04.60,0:11:05.50,tomb,,0,0,0,,Licik!
Dialogue: 0,0:11:06.13,0:11:08.33,tomb,,0,0,0,,Berani meracuni!
Dialogue: 0,0:11:09.30,0:11:12.30,tomb,,0,0,0,,Dendam pembunuhan, kenapa tidak dibalas?
Dialogue: 0,0:11:25.43,0:11:27.33,tomb,,0,0,0,,Kau benar!
Dialogue: 0,0:11:28.20,0:11:30.33,tomb,,0,0,0,,Aku memang sangat membenci Jalan Pemusnah,
Dialogue: 0,0:11:31.03,0:11:32.50,tomb,,0,0,0,,hari ini dendam lama dan baru,
Dialogue: 0,0:11:33.20,0:11:34.23,tomb,,0,0,0,,akan kuselesaikan.
Dialogue: 0,0:11:34.80,0:11:36.43,tomb,,0,0,0,,Sebab akibatku dengan Jalan Pemusnah,
Dialogue: 0,0:11:37.03,0:11:38.23,tomb,,0,0,0,,kau takkan sanggup menanggung.
Dialogue: 0,0:11:45.83,0:11:47.33,tomb,,0,0,0,,Awalnya tak perlu membunuhmu,
Dialogue: 0,0:11:50.57,0:11:52.03,tomb,,0,0,0,,tapi kau seharusnya tidak menyebut.
Dialogue: 0,0:11:55.73,0:11:57.90,tomb,,0,0,0,,Jalan Pemusnah.
Dialogue: 0,0:12:02.13,0:12:03.33,tomb,,0,0,0,,Jalan Pemusnah,
Dialogue: 0,0:12:03.93,0:12:05.23,tomb,,0,0,0,,dan Jalan Kekacauan,
Dialogue: 0,0:12:06.03,0:12:06.83,tomb,,0,0,0,,tidak akan,
Dialogue: 0,0:12:07.30,0:12:09.30,tomb,,0,0,0,,membiarkanmu.
Dialogue: 0,0:12:30.70,0:12:32.10,tomb,,0,0,0,,Mau membunuhku?
Dialogue: 0,0:12:33.43,0:12:34.83,tomb,,0,0,0,,Mimpi!
Dialogue: 0,0:12:36.13,0:12:36.83,tomb,,0,0,0,,Chen Nan,
Dialogue: 0,0:12:37.80,0:12:39.13,tomb,,0,0,0,,dendamku denganmu,
Dialogue: 0,0:12:39.83,0:12:41.10,tomb,,0,0,0,,takkan terampuni.
Dialogue: 0,0:12:42.20,0:12:43.60,tomb,,0,0,0,,Saat lukaku sembuh,
Dialogue: 0,0:12:44.20,0:12:46.73,tomb,,0,0,0,,itulah saat kematianmu.
Dialogue: 0,0:12:49.20,0:12:49.60,tomb,,0,0,0,,Chen Nan,
Dialogue: 0,0:12:50.17,0:12:51.10,tomb,,0,0,0,,kenapa tadi kau seperti itu?
Dialogue: 0,0:12:51.77,0:12:52.30,tomb,,0,0,0,,Sepertinya,
Dialogue: 0,0:12:52.57,0:12:53.30,tomb,,0,0,0,,ada yang tidak beres.
Dialogue: 0,0:12:54.13,0:12:55.70,tomb,,0,0,0,,Aku punya dendam lama dengan Jalan Pemusnah.
Dialogue: 0,0:12:56.33,0:12:57.20,tomb,,0,0,0,,Emosiku tak terkendali.
Dialogue: 0,0:12:57.50,0:12:58.13,tomb,,0,0,0,,Lebih baik tidak dibahas.
Dialogue: 0,0:12:59.60,0:12:59.93,tomb,,0,0,0,,Begitu ya.
Dialogue: 0,0:13:08.13,0:13:08.73,tomb,,0,0,0,,Hm.
Dialogue: 0,0:13:13.03,0:13:13.50,tomb,,0,0,0,,Ngomong-ngomong,
Dialogue: 0,0:13:14.33,0:13:15.53,tomb,,0,0,0,,sepasang sayap dewa di tubuhnya,
Dialogue: 0,0:13:15.97,0:13:17.10,tomb,,0,0,0,,juga harta langka yang jarang ditemui!
Dialogue: 0,0:13:19.37,0:13:19.97,tomb,,0,0,0,,Ke mana Belut itu pergi?
Dialogue: 0,0:13:20.20,0:13:20.93,tomb,,0,0,0,,Sepertinya,
Dialogue: 0,0:13:21.20,0:13:22.63,tomb,,0,0,0,,saat kau bertarung dengan Xiang Tian,
Dialogue: 0,0:13:22.70,0:13:23.23,tomb,,0,0,0,,dia sudah terbang pergi.
Dialogue: 0,0:13:25.63,0:13:27.73,tomb,,0,0,0,,Aku sudah menduga dia akan kabur,
Dialogue: 0,0:13:28.30,0:13:29.33,tomb,,0,0,0,,sudah menemukan,
Dialogue: 0,0:13:29.40,0:13:31.73,tomb,,0,0,0,,tempat persembunyiannya di Kota Pingyang.
Dialogue: 0,0:13:32.33,0:13:33.10,tomb,,0,0,0,,Lalu kenapa kau masih menunggu?
Dialogue: 0,0:13:33.57,0:13:34.43,tomb,,0,0,0,,Cepat bawa kami ke kota!
Dialogue: 0,0:13:35.40,0:13:36.10,tomb,,0,0,0,,Gadis kecil,
Dialogue: 0,0:13:36.43,0:13:38.53,tomb,,0,0,0,,kau benar-benar menganggapku binatang penarik kereta?
Dialogue: 0,0:13:44.03,0:13:45.80,tomb,,0,0,0,,Kakek Naga sudah lama berkeliaran,
Dialogue: 0,0:13:46.03,0:13:47.83,tomb,,0,0,0,,tidak pernah mengalami perlakuan seperti ini.
Dialogue: 0,0:13:48.33,0:13:49.20,tomb,,0,0,0,,Aku tidak peduli,
Dialogue: 0,0:13:49.53,0:13:51.97,tomb,,0,0,0,,Sayap Dewa Iblis-nya harus diserahkan padaku.
Dialogue: 0,0:13:52.80,0:13:53.60,tomb,,0,0,0,,Kau ini,
Dialogue: 0,0:13:54.10,0:13:55.73,tomb,,0,0,0,,apa benar-benar Naga Suci Timur?
Dialogue: 0,0:13:56.43,0:13:58.50,tomb,,0,0,0,,Kok juga suka mengumpulkan harta berkilau?
Dialogue: 0,0:13:58.93,0:13:59.37,tomb,,0,0,0,,Cih!
Dialogue: 0,0:13:59.77,0:14:01.10,tomb,,0,0,0,,Kadal barat itu,
Dialogue: 0,0:14:01.30,0:14:02.37,tomb,,0,0,0,,bisa dibandingkan dengan Kakek?
Dialogue: 0,0:14:02.90,0:14:04.97,tomb,,0,0,0,,Sayap Dewa Iblis sama seperti Giok Teratai,
Dialogue: 0,0:14:05.43,0:14:06.97,tomb,,0,0,0,,dibuat dengan teknik Taois,
Dialogue: 0,0:14:07.13,0:14:08.80,tomb,,0,0,0,,harta tingkat tinggi yang disempurnakan.
Dialogue: 0,0:14:09.53,0:14:12.30,tomb,,0,0,0,,Jika aku bisa memahami formasi dalamnya,
Dialogue: 0,0:14:12.43,0:14:13.83,tomb,,0,0,0,,itu benar-benar...
Dialogue: 0,0:14:14.83,0:14:29.83,tomb,,1,1,0,,Join channel telegram @dongworld agar tidak ketinggalan update.\NDonghua ini diterjemahkan oleh Dongworld.
Dialogue: 0,0:14:40.33,0:14:41.30,tomb,,0,0,0,,Belut, kau sedang apa?
Dialogue: 0,0:14:42.00,0:14:42.97,tomb,,0,0,0,,jangan berbuat aneh-aneh,
Dialogue: 0,0:14:43.43,0:14:44.77,tomb,,0,0,0,,terlalu berbahaya di ketinggian ini.
Dialogue: 0,0:14:44.93,0:14:45.80,tomb,,0,0,0,,Aduh,
Dialogue: 0,0:14:45.90,0:14:47.63,tomb,,0,0,0,,benar-benar niat baik dianggap jahat.
Dialogue: 0,0:14:48.20,0:14:49.83,tomb,,0,0,0,,Kakek ingin menolong kalian,
Dialogue: 0,0:14:50.13,0:14:51.23,tomb,,0,0,0,,kalian tidak berterima kasih,
Dialogue: 0,0:14:51.43,0:14:52.73,tomb,,0,0,0,,malah menyalahkanku.
Dialogue: 0,0:14:54.43,0:14:55.17,tomb,,0,0,0,,Aduh,
Dialogue: 0,0:14:55.70,0:14:57.30,tomb,,0,0,0,,Adik, lebih keras lagi pukulannya.
Dialogue: 0,0:14:57.90,0:14:59.10,tomb,,0,0,0,,Kubunuh kau!
Dialogue: 0,0:15:00.13,0:15:01.13,tomb,,0,0,0,,Sudah, sudah,
Dialogue: 0,0:15:01.70,0:15:02.53,tomb,,0,0,0,,serius sedikit!
Dialogue: 0,0:15:12.70,0:15:13.23,tomb,,0,0,0,,Masih hangat.
Dialogue: 0,0:15:16.00,0:15:18.40,tomb,,0,0,0,,Aku sudah lama menemukan naga aneh itu mengikuti dari belakang,
Dialogue: 0,0:15:18.73,0:15:19.83,tomb,,0,0,0,,kalian pasti gagal.
Dialogue: 0,0:15:20.60,0:15:21.63,tomb,,0,0,0,,Tunggu lukaku sembuh,
Dialogue: 0,0:15:22.20,0:15:23.83,tomb,,0,0,0,,Aku pasti akan membalas dendam atas aib ini.
Dialogue: 0,0:15:24.43,0:15:26.80,tomb,,0,0,0,,Dia hampir dicincang seperti isi pangsit,
Dialogue: 0,0:15:27.03,0:15:28.50,tomb,,0,0,0,,hanya mulutnya yang masih keras.
Dialogue: 0,0:15:32.37,0:15:34.13,tomb,,0,0,0,,Beberapa kilometer dari sini,
Dialogue: 0,0:15:34.53,0:15:38.97,tomb,,0,0,0,,sudah kurasakan niat membunuh dari teknik kultivasimu yang misterius.
Dialogue: 0,0:15:41.80,0:15:43.10,tomb,,0,0,0,,Pemuda,
Dialogue: 0,0:15:43.57,0:15:45.84,tomb,,0,0,0,,kita bertemu lagi.
